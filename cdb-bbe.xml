<?xml version="1.0" encoding="utf-8"?>
<cdb xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb">
  <class frame="bbe" id="bbe" label="Helium Compressor">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <command name="getstat" />
      </session>
      <session id="x0on" label="X0 ON">
        <command name="getstat"></command>
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x01</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x0off" label="X0 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xfffe</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x1on" label="X1 ON">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x10</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x1off" label="X1 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xffef</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x2on" label="X2 ON">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x100</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x2off" label="X2 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xfeff</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x3on" label="X3 ON">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x1000</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x3off" label="X3 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xefff</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x01on" label="X0 and X1 ON">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x0011</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x01off" label="X0 and X1 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xffee</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x23on" label="X2 and X3 ON">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}|0x1100</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
      <session id="x23off" label="X2 and X3 OFF">
        <command name="getstat" />
        <command name="set">
          <argv>stat</argv>
          <argv>${stat}&amp;0xeeff</argv>
        </command>
        <command name="outfld" />
        <command name="getstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <repeat from="0" to="3">
        <row label="Group">
          <value id="xsw%d" label="SWITCH %d" symbol="off-warn">
            <binary bit="$_*4" ref="stat" />
          </value>
          <value id="xdr%d" label="DRIVE %d" symbol="off-warn">
            <binary bit="$_*4+1" ref="stat" />
          </value>
          <value id="xtm%d" label="TEMP ALARM %d" symbol="alarm">
            <binary bit="$_*4+2" ref="stat" />
          </value>
          <value id="xpr%d" label="PRES ALARM %d" symbol="alarm">
            <binary bit="$_*4+3" ref="stat" />
          </value>
        </row>
      </repeat>
    </status>
  </class>
</cdb>
