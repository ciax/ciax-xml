        </symbol>
      </var>
      <var id="2dr" label="[CH2] HC Drive" type="ENUM">
        <fields>
          <binary ref="stat" bit="9" />
        </fields>
        <symbol>
          <enum hl="warn" msg="OFF">1</enum>
          <enum msg="ON" hl="normal">0</enum>
        </symbol>
      </var>
      <var id="2tm" label="[CH2] HC TMP Alarm" type="ENUM">
        <fields>
          <binary ref="stat" bit="10" />
        </fields>
        <symbol>
          <enum hl="hide" msg="OK">1</enum>
          <enum msg="ALARM" hl="alarm">0</enum>
        </symbol>
      </var>
      <var id="2pr" label="[CH2] HC PRS Alarm" type="ENUM">
