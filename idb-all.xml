  <project id="cmp" caption="Helium Compressor" column="2">
    <instance id="cro" app_id="bbe" host="ciax" port="25106" label="OPT Helium Compressor">
      <alias xmlns="http://ciax.sum.naoj.org/ciax-xml/idbc">
        <item id="on" label="Alias for X0 ON" ref="x0on" />
        <item id="off" label="Alias for X0 OFF" ref="x0off" />
      </alias>
      <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
        <group ref="g0" caption="X0-3" />
        <group ref="g1" caption="X4-7" />
        <group ref="g2" caption="X8-11" />
        <group ref="g3" caption="X12-15" />
      </status>
      <watch xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb" interval="1">
        <event label="ON Executed" id="on">
          <onchange var="xsw0"/>
          <pattern var="xsw0">1</pattern>
          <pattern var="xsw1">0</pattern>
          <exec name="x1on"/>
        </event>
        <event label="OFF Executed" id="off">
          <onchange var="xsw0"/>
          <pattern var="xsw0">0</pattern>
          <pattern var="xsw1">1</pattern>
          <exec name="x1off"/>
        </event>
      </watch>
    </instance>
