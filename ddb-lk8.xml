<?xml version='1.0' encoding='ISO-8859-1'?>
<ddb xsi:schemaLocation='http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd' xmlns='http://ciax.sum.naoj.org/ciax-xml/ddb' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
  <device id='lk8' label='Lakeshore MODEL 340 8CH' wait='0.3'>
    <serial databit='7' flowctl='NONE' parity='NONE' speed='9600' stopbit='1'/>
    <cmdselect>
      <command id='getstat' label='Get Status' response='getstat' type='stat'>
        <data text='krdg? c1;' type='text'/>
        <data text='krdg? c2;' type='text'/>
        <data text='krdg? c3;' type='text'/>
        <data text='krdg? c4;' type='text'/>
        <data text='krdg? d1;' type='text'/>
        <data text='krdg? d2;' type='text'/>
        <data text='krdg? d3;' type='text'/>
        <data text='krdg? d4;' type='text'/>
      </command>
      <command id='htrstat' label='Heater Power (%)' response='htrstat' type='stat'>
        <data text='HTR?' type='text'/>
      </command>
      <command id='spstat' label='Set Point (K)' response='spstat' type='stat'>
        <data text='SETP? 1' type='text'/>
      </command>
    </cmdselect>
    <cmdframe>
      <selected/>
      <data encode='bew' label='Suffix' text='0x0d0a' type='text'/>
    </cmdframe>
    <rspselect>
      <response id='getstat'>
        <array assign='ch'>
          <index range='0:7' size='8'/>
        </array>
      </response>
      <response id='htrstat'>
        <field assign='htr'/>
      </response>
      <response id='spstat'>
        <field assign='sp'/>
      </response>
    </rspselect>
    <rspframe delimiter=';' terminator='\r\n'>
      <selected/>
    </rspframe>
  </device>
</ddb>
