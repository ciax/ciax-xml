        </fields>
        <symbol>
          <enum hl="warn" msg="OFF">1</enum>
          <enum msg="ON" hl="normal">0</enum>
        </symbol>
      </var>
      <var id="3tm" label="[CH3] HC TMP Alarm" type="ENUM">
