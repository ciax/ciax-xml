<?xml version="1.0" encoding="utf-8"?>
<cdb xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb">
  <class frame="oma" id="oma" label="Compressed Air Pressure at MOIRCS">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <cmdset id="upd" label="Update Status">
        <command name="getstat" />
      </cmdset>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <row label="Group">
        <value id="pres" label="Air Pressure (Psi)" symbol="pres">
          <float format="%4.2f" ref="stat" />
        </value>
      </row>
    </status>
    <watch interval="2" xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb">
      <periodic label="Update" period="10">
        <command name="getstat" />
      </periodic>
    </watch>
  </class>
  <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
    <table id="pres" type="range">
      <case class="alarm" msg="HH">110:</case>
      <case class="warn" msg="H">105:&lt;110</case>
      <case class="normal" msg="M">95:&lt;105</case>
      <case class="warn" msg="L">90:&lt;95</case>
      <case class="alarm" msg="LL">:&lt;90</case>
    </table>
  </symbol>
</cdb>
