<?xml version="1.0" encoding="UTF-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd">
  <frame id="oma" label="OMEGA Air Pressure Sensor(PX971)" wait="0.1">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1"/>
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">*</string>
      <select>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <string>00P1</string>
        </command>
      </select>
      <code length="1" label="Suffix">0xd</code>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r">
      <field label="Prefix" length="1">?</field>
      <select>
        <response id="getstat">
          <field length="5">01CP=</field>
          <field assign="stat" label="Status" length="6"/>
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
