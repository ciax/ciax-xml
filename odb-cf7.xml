<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ciax.sum.naoj.org/ciax-xml" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="cf7" label="S7 Helium Compressor Coolant Flow" device="k3n" client="nc ltt-o 4004" server="socat - udp-l:45601,reuseaddr,fork">
    <controls>
      <session id="upd" label="Update Status">
        <command text="getstat" />
      </session>
    </controls>
    <status>
      <var id="flow" label="HDS(S7) Coolant Flow(l/min)" type="FLOAT">
        <fields>
          <float decimal="2" format="%4.2f" ref="stat" />
        </fields>
        <symbol type="range">
          <range hl="alarm" msg="HH" min="6"></range>
          <range hl="warn" msg="H" min="5"></range>
          <range hl="normal" msg="M" min="4"></range>
          <range hl="warn" msg="L" min="3"></range>
          <range hl="alarm" msg="LL"></range>
        </symbol>
      </var>
    </status>
  </object>
</odb>