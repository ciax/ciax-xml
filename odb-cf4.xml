<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ciax.sum.naoj.org/ciax-xml" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="cf4" label="S4 Helium Compressor Coolant Flow" device="k3n" client="nc ltt-o 4001" server="socat - udp-l:45604,reuseaddr,fork">
    <controls>
      <session id="upd" label="Update Status">
        <command text="getstat" />
      </session>
    </controls>
    <status>
      <var id="flow" label="COMICS/IMAGE(S4) Coolant Flow(l/min)" type="FLOAT">
        <fields>
          <float decimal="2" format="%4.2f" ref="stat" />
        </fields>
        <symbol hl="alarm" msg="LL">
          <range_min hl="alarm" msg="HH">6</range_min>
          <range_min hl="warn" msg="H">5</range_min>
          <range_min hl="normal" msg="M">4</range_min>
          <range_min hl="warn" msg="L">3</range_min>
        </symbol>
      </var>
    </status>
  </object>
</odb>