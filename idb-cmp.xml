          <group ref="g2" caption="COMICS/IMG(S4)"/>
          <group ref="g3" caption="COMICS/SPEC(S5)"/>
        </status>
      </site>
      <site id="cro" dev_id="cro" app_id="bbe" port="53205" label="IR Helium Compressor">
