<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd">
  <frame id="cj2" label="OMRON CJ2M-CPU33">
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <data encode="chr" label="FINS Header:ICF">0x80</data>
      <data encode="chr" label="FINS Header:RSV">0x00</data>
      <data encode="chr" label="FINS Header:GCT">0x02</data>
      <data encode="chr" label="FINS Header:DNA">0x00</data>
      <data encode="chr" label="FINS Header:DA1">${ipr}</data>
      <data encode="chr" label="FINS Header:DA2">0x00</data>
      <data encode="chr" label="FINS Header:SNA">0x00</data>
      <data encode="chr" label="FINS Header:SA1">${ipl}</data>
      <data encode="chr" label="FINS Header:SA2">0x00</data>
      <data encode="chr" label="FINS Header:SID">0xFF</data>
      <select>
        <command id="getstat" label="Get all status" response="getstat" type="stat">
          <data encode="chr" label="FINS Command:MRC">0x01</data>
          <data encode="chr" label="FINS Command:SRC">0x01</data>
          <data encode="chr" label="FINS Param:Memory type(W)">0xB1</data>
          <data encode="chr" label="FINS Param:Start Address(H)">0x00</data>
          <data encode="chr" label="FINS Param:Start Address(L)">0x0A</data>
          <data encode="chr" label="FINS Param:Bit Number">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (H)">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (L)">0x14</data>
        </command>
        <command id="geterr" label="Get Error Code" response="geterr" type="stat">
          <data encode="chr" label="FINS Command:MRC">0x01</data>
          <data encode="chr" label="FINS Command:SRC">0x01</data>
          <data encode="chr" label="FINS Param:Memory type(D)">0x82</data>
          <data encode="chr" label="FINS Param:Start Address(H)">0x00</data>
          <data encode="chr" label="FINS Param:Start Address(L)">0x00</data>
          <data encode="chr" label="FINS Param:Bit Number">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (H)">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (L)">0x01</data>
        </command>
        <command id="getzlen" label="Get Z Sensor Length" response="getzlen" type="stat">
          <data encode="chr" label="FINS Command:MRC">0x01</data>
          <data encode="chr" label="FINS Command:SRC">0x01</data>
          <data encode="chr" label="FINS Param:Memory type(D)">0x82</data>
          <data encode="chr" label="FINS Param:Start Address(H)">0x00</data>
          <data encode="chr" label="FINS Param:Start Address(L)">0x0A</data>
          <data encode="chr" label="FINS Param:Bit Number">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (H)">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (L)">0x0A</data>
        </command>
        <command id="setbit" label="Set Bit" response="cmdrsp" type="stat">
          <data encode="chr" label="FINS Command:MRC">0x01</data>
          <data encode="chr" label="FINS Command:SRC">0x02</data>
          <data encode="chr" label="FINS Param:Memory type(W)">0x31</data>
          <data encode="chr" label="FINS Param:Start Address(H)">0x00</data>
          <data encode="chr" label="FINS Param:Start Address(L)" valid="0:3">$1</data>
          <data encode="chr" label="FINS Param:Bit Number" valid="0:15">$2</data>
          <data encode="chr" label="FINS Param:Bit Data Length (H)">0x00</data>
          <data encode="chr" label="FINS Param:Bit Data Length (L)">0x01</data>
          <data encode="chr" label="FINS Param:Bit Data">0x01</data>
        </command>
        <command id="clrbit" label="Clear All Bit" response="cmdrsp" type="stat">
          <data encode="chr" label="FINS Command:MRC">0x01</data>
          <data encode="chr" label="FINS Command:SRC">0x02</data>
          <data encode="chr" label="FINS Param:Memory type(W)">0xB1</data>
          <data encode="chr" label="FINS Param:Start Address(H)">0x00</data>
          <data encode="chr" label="FINS Param:Start Address(L)">0x00</data>
          <data encode="chr" label="FINS Param:Bit Number">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (H)">0x00</data>
          <data encode="chr" label="FINS Param:Data Length (L)">0x04</data>
          <data encode="bew" label="FINS Param:Bit Data">0x00</data>
          <data encode="bew" label="FINS Param:Bit Data">0x00</data>
          <data encode="bew" label="FINS Param:Bit Data">0x00</data>
          <data encode="bew" label="FINS Param:Bit Data">0x00</data>
        </command>
      </select>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field decode="chr" label="FINS Header:ICF" length="1">0xc0</field>
      <field decode="chr" label="FINS Header:RSV" length="1">0x00</field>
      <field decode="chr" label="FINS Header:GCT" length="1">0x02</field>
      <field decode="chr" label="FINS Header:DNA" length="1">0x00</field>
      <field decode="chr" label="FINS Header:DA1" length="1" assign="ipl" />
      <field decode="chr" label="FINS Header:DA2" length="1">0x00</field>
      <field decode="chr" label="FINS Header:SNA" length="1">0x00</field>
      <field decode="chr" label="FINS Header:SA1" length="1" assign="ipr" />
      <field decode="chr" label="FINS Header:SA2" length="1">0x00</field>
      <field decode="chr" label="FINS Header:SID" length="1">0xFF</field>
      <select>
        <response id="getstat">
          <field decode="chr" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="chr" label="FINS Command:SRES" length="1">0x00</field>
          <array assign="w" decode="bew" label="Status" length="2">
            <index range="0:19" size="20" />
          </array>
        </response>
        <response id="geterr">
          <field decode="chr" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="chr" label="FINS Command:SRES" length="1">0x00</field>
          <field assign="err" decode="bew" label="FINS Command:Error Code" length="2" />
        </response>
        <response id="getzlen">
          <field decode="chr" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="chr" label="FINS Command:SRES" length="1">0x00</field>
          <array assign="d" decode="bew" label="Status" length="2">
            <index range="0:9" size="8" />
          </array>
        </response>
        <response id="cmdrsp" label="command resp">
          <field decode="chr" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="chr" label="FINS Command:SRC" length="1">0x02</field>
          <field decode="chr" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="chr" label="FINS Command:SRES" length="1">0x00</field>
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
