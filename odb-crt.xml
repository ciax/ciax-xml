<?xml version="1.0" encoding="UTF-8"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="crt" label="Cart PLC">
    <comm>
      <class>mel</class>
      <client>socat - udp:cart3:5000</client>
      <server>socat - udp-l:25101,reuseaddr,fork</server>
    </comm>
    <status>
      <group>
        <title ref="ecode" symbol="mel:ecode" label="EXIT CODE"></title>
        <title ref="rdy" symbol="normal" label="READY"></title>
        <title ref="pse" symbol="warn" label="PAUSE"></title>
        <title ref="emg" symbol="alarm" label="ENERGENCY"></title>
        <title ref="stp" symbol="warn" label="STOP"></title>
      </group>
      <group>
        <title ref="run" symbol="normal" label="RUNNING"></title>
        <title ref="jak" symbol="normal" label="JACKING"></title>
        <title ref="con" symbol="normal" label="CHARGING"></title>
        <title ref="flt" symbol="alarm" label="Fault"></title>
      </group>
      <group>
        <title ref="msr" symbol="normal" label="Right Motor Servo"></title>
        <title ref="msl" symbol="normal" label="Left Motor Servo"></title>
      </group>
      <group>
        <title ref="jd1" symbol="normal" label="Jack Motor1 Servo"></title>
        <title ref="jd2" symbol="normal" label="Jack Motor2 Servo"></title>
      </group>
      <group>
        <title ref="jd3" symbol="normal" label="Jack Motor3 Servo"></title>
        <title ref="jd4" symbol="normal" label="Jack Motor4 Servo"></title>
      </group>
      <group>
        <title ref="mdar" symbol="mel:motor" label="Rignt Motor D/A"></title>
        <title ref="mdal" symbol="mel:motor" label="Left Motor D/A"></title>
      </group>
      <group>
        <title ref="bvl" symbol="mel:battery" label="Battery Voltage"></title>
        <title ref="ist" symbol="mel:inst" label="Instrument"></title>
      </group>
      <group>
        <title ref="cpos" label="Current Position"></title>
        <title ref="dpos" label="Destination Position"></title>
      </group>
      <group>
        <title ref="jpos" symbol="mel:jpos" label="Jack Position"></title>
      </group>
    </status>
  </object>
</odb>
