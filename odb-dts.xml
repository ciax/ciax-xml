<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:cxws:9999" device="oss" id="dts" label="TSC Control" server="socat - udp-l:25105,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>iststat</text>
        </command>
        <command>
          <text>jakstat</text>
        </command>
        <command>
          <text>inrstat</text>
        </command>
        <command>
          <text>logstat</text>
        </command>
        <command>
          <text>exitcode</text>
        </command>
      </session>
      <session id="request" label="Start TCP Status Request">
        <command>
          <text>osscmd</text>
          <text>1A1011</text>
        </command>
      </session>
      <session id="login" label="Login">
        <command label="TSC Login">
          <text>login</text>
        </command>
        <command label="TSC Only Mode(CIAX Mode)">
          <text>osscmd</text>
          <text>1A1008TSCONLY</text>
        </command>
      </session>
      <session id="logout" label="Logout">
        <command label="TSC Primaly Mode">
          <text>osscmd</text>
          <text>1A1008TSCPRI%</text>
        </command>
        <command label="TSC Logout">
          <text>osscmd</text>
          <text>1A1902</text>
        </command>
      </session>
    </selection>
    <status>
      <var id="exit" label="Exit Code" trail="true">
        <int field="exit" />
      </var>
    </status>
    <symbols>
      <symbol id="inst" type="ENUM">
        <case hl="warn" msg="NONE">0</case>
        <case hl="normal" msg="CIAX3">1</case>
        <case hl="normal" msg="COMICS">2</case>
        <case hl="normal" msg="CIAO">3</case>
        <case hl="normal" msg="FOCAS">4</case>
        <case hl="normal" msg="IRCS">5</case>
        <case hl="normal" msg="N/A">6</case>
        <case hl="normal" msg="MOIRCS">7</case>
        <case hl="normal" msg="MP-CS1">8</case>
        <case hl="normal" msg="MP-CS2">9</case>
      </symbol>
    </symbols>
  </object>
</odb>