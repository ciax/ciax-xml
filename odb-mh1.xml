<?xml version="1.0" encoding="utf-8"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="mh1" label="MOIRCS Ext Heater 1">
    <comm>
      <class>psp</class>
      <client>socat - tcp:merope:59999</client>
      <server>socat - udp-l:24201,reuseaddr,fork</server>
    </comm>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/odbs">
      <group>
        <title ref="vol" label="OUTPUT (V)" />
        <title ref="amp" label="OUTPUT (A)" />
        <title ref="wat" label="OUTPUT (W)" />
      </group>
      <group>
        <title ref="vl" label="Limit (V)" />
        <title ref="il" label="Limit (A)" />
        <title ref="pl" label="Limit (W)" />
      </group>
      <group>
        <title ref="sw" symbol="warn" label="OUTPUT ON/OFF" />
        <title ref="tmp" symbol="alarm" label="OverHeat" />
      </group>
      <group>
        <title ref="kn1" symbol="normal" label="Wheel Knov Fine" />
        <title ref="kn2" symbol="normal" label="Wheel Knov Unlock" />
      </group>
      <group>
        <title ref="rem" symbol="normal" label="REMOTE" />
        <title ref="loc" symbol="normal" label="Local Lock" />
      </group>
    </status>
  </object>
</odb>