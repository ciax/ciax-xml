<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="srm" id="srm" label="CsEtr Controller">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="getstat" />
      </session>
      <session id="c3on" label="CIAX3 POWER ON">
        <statement command="fset" label="Bit 0 on">
          <argv>5</argv>
        </statement>
      </session>
      <session id="c3off" label="CIAX3 POWER OFF">
        <statement command="fset" label="Bit 0 OFF">
          <argv>4</argv>
        </statement>
      </session>
      <session id="emon" label="CIAX3 EMERGENCY ON">
        <statement command="fset" label="Bit 1 ON">
          <argv>50</argv>
        </statement>
      </session>
      <session id="emoff" label="CIAX3 EMERGENCY OFF">
        <statement command="fset" label="Bit 1 OFF">
          <argv>40</argv>
        </statement>
      </session>
      <session id="ccdon" label="CS CCD ON">
        <statement command="fset" label="Bit 2 ON">
          <argv>500</argv>
        </statement>
      </session>
      <session id="ccdoff" label="CS CCD OFF">
        <statement command="fset" label="Bit 2 OFF">
          <argv>400</argv>
        </statement>
      </session>
      <session id="lanon" label="CS LAN ON">
        <statement command="fset" label="Bit 3 ON">
          <argv>5000</argv>
        </statement>
      </session>
      <session id="lanoff" label="CS LAN OFF">
        <statement command="fset" label="Bit 3 OFF">
          <argv>4000</argv>
        </statement>
      </session>
      <session id="ipwon" label="CS IPW ON">
        <statement command="fset" label="Bit 4 ON">
          <argv>50000</argv>
        </statement>
      </session>
      <session id="ipwoff" label="CS IPW OFF">
        <statement command="fset" label="Bit 4 OFF">
          <argv>40000</argv>
        </statement>
      </session>
      <session id="ston" label="STOW PIN ON">
        <statement command="fset" label="Bit 5 ON">
          <argv>500000</argv>
        </statement>
      </session>
      <session id="stoff" label="STOW PIN OFF">
        <statement command="fset" label="Bit 5 OFF">
          <argv>400000</argv>
        </statement>
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <value format="%s" id="iid" label="CAS INST ID" group="1">
        <string ref="c1d2" />
      </value>
      <value format="%s" id="ipw" label="CAS IPW" group="1">
        <binary bit="0" ref="c1d1" />
      </value>
      <value format="%s" id="stp" label="STOW PIN" group="2">
        <binary bit="1" ref="c1d1" />
      </value>
      <value format="%s" id="ccd" label="CAS CCD PW" group="2">
        <binary bit="2" ref="c1d0" />
      </value>
      <value format="%s" id="lan" label="CAS LAN PW" group="2">
        <binary bit="3" ref="c1d0" />
      </value>
      <value format="%s" id="c3p" label="CIAX3 PW" group="3">
        <binary bit="0" ref="c1d0" />
      </value>
      <value format="%s" id="c3e" label="CIAX3 EMG" group="3">
        <binary bit="1" ref="c1d0" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <enum id="iid" ref="iid_ac" />
      <enum id="ipw" ref="off-warn" />
      <enum id="stp" ref="warn" />
      <enum id="ccd" ref="warn" />
      <enum id="lan" ref="hide" />
      <enum id="c3p" ref="hide" />
      <enum id="c3e" ref="alarm" />
    </symbol>
  </class>
</cdb>