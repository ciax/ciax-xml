<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="fpa" label="Field Point Analog Input(FP-AI-100)" version="1">
    <spec brand="Field Point" type="Analog Input" model="FP-AI-100" reference="http://ciax.sum.naoj.org/man/fieldpoint/fp-1000prm-321632c.pdf"/>
    <stream ccmethod="sum" wait="0.01" terminator="\r"/>
    <serial databit="8" flowctl="NONE" parity="NONE" speed="38400" stopbit="1"/>
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">&gt;01</string>
      <body/>
      <string>??</string>
      <char length="1">13</char>
    </cmdframe>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <group id="gst" caption="Status">
        <!--String Length must be <32-->
        <item id="getstat" label="Get status" response="getstat" type="stat">
          <string>!F00FF</string>
        </item>
        <item id="getcfg" label="Get config" response="getcfg" type="stat">
          <string>!E00FF</string>
          <repeat from="1" to="8">
            <string>00001</string>
          </repeat>
        </item>
      </group>
      <group id="gcfg" caption="Configuration">
        <item id="setr1" label="Set range +-1.2V" response="default" type="config">
          <string>!D00FF</string>
          <repeat from="1" to="8">
            <string>0000107</string>
          </repeat>
        </item>
        <item id="setr6" label="Set range +-6V(default)" response="default" type="config">
          <string>!D00FF</string>
          <repeat from="1" to="8">
            <string>0000105</string>
          </repeat>
        </item>
        <item id="setr15" label="Set range +-15V" response="default" type="config">
          <string>!D00FF</string>
          <repeat from="1" to="8">
            <string>0000112</string>
          </repeat>
        </item>
      </group>
    </command>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <string label="Prefix">A</string>
      <ccrange>
        <body/>
      </ccrange>
      <hexstr assign="cc" label="BCC" length="2"/>
    </rspframe>
    <response xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <item id="getstat">
        <hexstr assign="stat" index="0:7" label="Status" length="4"/>
      </item>
      <item id="getcfg">
        <hexstr assign="cfg" index="0:7" label="Config" length="2"/>
      </item>
      <item id="default" noaffix="true"/>
    </response>
    <field xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <var id="cc" label="BCC"/>
      <array id="stat" label="Status">
        <index size="8"/>
      </array>
      <array id="cfg" label="Config">
        <index size="8"/>
      </array>
    </field>
  </frame>
</fdb>
