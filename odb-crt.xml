<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="crt" label="Cart PLC" device="mel" client="socat - udp:cart3:5000" server="socat - udp-l:25101,reuseaddr,fork">
    <selection>
      <session id="upd" label="Regular status update">
        <command text="getstat" />
        <command text="chken" />
      </session>
      <session id="start_run" label="Running Start">
        <command text="start">
          <par type="text">1</par>
        </command>
      </session>
      <session id="start_jak" label="Jacking Start">
        <command text="start">
          <par type="text">0x100</par>
        </command>
      </session>
      <session id="clear_trg" label="Jacking Start">
        <command text="clear" />
      </session>
      <session id="set_fwd" label="Set Tag to Forward">
        <command text="set_tag">
          <par type="text">98</par>
        </command>
      </session>
      <session id="set_run1" label="Run to Tag 1">
        <command text="set_tag">
          <par type="text">1</par>
        </command>
      </session>
      <session id="set_jak1" label="Jack UP Level 1">
        <command text="set_pos">
          <par type="text">2</par>
        </command>
      </session>
      <session id="pause" label="Pause">
        <command text="stop">
          <par type="text">1</par>
        </command>
      </session>
      <session id="emstop" label="Emergency Stop">
        <command text="stop">
          <par type="text">2</par>
        </command>
      </session>
      <session id="resume" label="Resume from Pause">
        <command text="clear" />
      </session>
    </selection>
    <status>
      <var id="rdy" label="READY" symbol="normal" trail="true">
        <binary bit="0" field="x00" />
      </var>
      <var id="pse" label="PAUSE" symbol="on-warn" trail="true">
        <binary bit="1" field="x00" />
      </var>
      <var id="emg" label="ENERGENCY" symbol="alarm" trail="true">
        <binary bit="2" field="x00" />
      </var>
      <var id="stp" label="STOP" symbol="on-warn">
        <binary bit="3" field="x00" />
      </var>
      <var id="run" label="RUNNING" symbol="normal" trail="true">
        <binary bit="4" field="x00" />
      </var>
      <var id="jak" label="JACKING" symbol="normal" trail="true">
        <binary bit="5" field="x00" />
      </var>
      <var id="con" label="CHARGING" symbol="normal" trail="true">
        <binary bit="6" field="x00" />
      </var>
      <var id="flt" label="Fault" symbol="alarm">
        <binary bit="7" field="x00" />
      </var>
      <var id="msr" label="Right Motor Servo" symbol="normal" trail="true">
        <binary bit="8" field="x00" inv="true" />
      </var>
      <var id="msl" label="Left Motor Servo" symbol="normal">
        <binary bit="9" field="x00" inv="true" />
      </var>
      <var id="jd1" label="Jack Motor1 Servo" symbol="normal" trail="true">
        <binary bit="10" field="x00" inv="true" />
      </var>
      <var id="jd2" label="Jack Motor2 Servo" symbol="normal">
        <binary bit="11" field="x00" inv="true" />
      </var>
      <var id="jd3" label="Jack Motor3 Servo" symbol="normal" trail="true">
        <binary bit="12" field="x00" inv="true" />
      </var>
      <var id="jd4" label="Jack Motor4 Servo" symbol="normal">
        <binary bit="13" field="x00" inv="true" />
      </var>
      <var id="mdar" label="Rignt Motor D/A" symbol="motor" trail="true">
        <int field="x07" signed="true" />
      </var>
      <var id="mdal" label="Left Motor D/A" symbol="motor">
        <int field="x08" signed="true" />
      </var>
      <var id="bvl" label="Battery Voltage" symbol="battery" trail="true">
        <float field="x16" decimal="2" format="%2.2f" />
      </var>
      <var id="ist" label="Instrument" symbol="inst">
        <int field="x17" />
      </var>
      <var id="cpos" label="Current Position" trail="true">
        <int field="x19" />
      </var>
      <var id="dpos" label="Destination Position">
        <int field="x1a" />
      </var>
      <var id="jpos" label="Jack Position" symbol="jpos">
        <int field="x1b" />
      </var>
    </status>
    <symbols>
      <symbol type="INT" id="motor">
        <range hl="alarm" msg="LOW">:10</range>
        <range hl="normal" msg="OK">10&lt;:30</range>
        <range hl="alarm" msg="HIGH">30&lt;:</range>
      </symbol>
      <symbol type="FLOAT" id="battery">
        <range hl="alarm" msg="LOW">:20</range>
        <range hl="warn" msg="LESS">20&lt;:22</range>
        <range hl="normal" msg="OK">22&lt;:26</range>
        <range hl="warn" msg="FULL">26&lt;:28</range>
        <range hl="alarm" msg="HIGH">28&lt;:</range>
      </symbol>
      <symbol type="ENUM" id="inst">
        <case hl="normal" msg="MOIRCS">2</case>
        <case hl="warn" msg="N/A"></case>
      </symbol>
      <symbol type="ENUM" id="jpos">
        <case hl="normal" msg="D">0</case>
        <case hl="normal" msg="1(L)">1</case>
        <case hl="normal" msg="2(M)">2</case>
        <case hl="normal" msg="3(H)">3</case>
        <case hl="normal" msg="CHG">4</case>
        <case hl="warn" msg="N/A"></case>
      </symbol>
    </symbols>
  </object>
</odb>