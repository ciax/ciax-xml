<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="oma" label="OMEGA Air Pressure Sensor(PX971)" wait="0.2">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">*</string>
      <body/>
      <char length="1" label="Suffix">0xd</char>
    </cmdframe>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <item id="getstat" label="Get Status" response="getstat" type="stat">
        <string>00P1</string>
      </item>
    </command>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r">
      <field label="Prefix">?</field>
      <body/>
    </rspframe>
    <responses xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <response id="getstat">
        <field label="Command Response">01CP=</field>
        <field assign="stat" label="Status" length="6" />
      </response>
    </responses>
  </frame>
</fdb>
