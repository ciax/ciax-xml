<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml">
  <device id="psp" label="Power Supply PSP-603" reference="http://www.bellnw.com/images/man/0685.pdf" wait="0.3">
    <serial speed="2400" databit="8" parity="NONE" stopbit="1" flowctl="NONE" />
    <selection>
      <session id="getstat" type="stat" label="Get Status">
        <send>
          <data>L</data>
        </send>
        <recv>
          <verify label="Format" length="1">V</verify>
          <assign length="5">v</assign>
          <verify label="Format" length="1">A</verify>
          <assign length="5">a</assign>
          <verify label="Format" length="1">W</verify>
          <assign length="5">w</assign>
          <verify length="1" label="Format">U</verify>
          <assign length="2">u</assign>
          <verify length="1" label="Format">I</verify>
          <assign length="4">i</assign>
          <verify length="1" label="Format">P</verify>
          <assign length="3">p</assign>
          <verify length="1" label="Format">F</verify>
          <assign length="1">f1</assign>
          <assign length="1">f2</assign>
          <assign length="1">f3</assign>
          <assign length="1">f4</assign>
          <assign length="1">f5</assign>
          <assign length="1">f6</assign>
        </recv>
      </session>
      <session id="setvl" type="config" label="Set Voltage Limit [DD]">
        <send>
          <data>SU </data>
          <par validate="range">0:99</par>
        </send>
      </session>
      <session id="setal" type="config" label="Set Current Limit [D.DD]">
        <send>
          <data>SI </data>
          <par validate="range">0:&lt;10</par>
        </send>
      </session>
      <session id="setpl" type="config" label="Set Power Limit [DDD]">
        <send>
          <data>SP </data>
          <par validate="range">0:999</par>
        </send>
      </session>
      <session id="pon" type="action" label="OUTPUT ON">
        <send>
          <data>KOE</data>
        </send>
      </session>
      <session id="poff" type="action" label="OUTPUT OFF">
        <send>
          <data>KOD</data>
        </send>
      </session>
      <session id="setv" type="config" label="Set Voltage [DD.DD]">
        <send>
          <data>SV </data>
          <par validate="range">0:&lt;100</par>
        </send>
      </session>
    </selection>
    <cmdframe>
      <selected />
      <data label="Suffix" encode="chr">13</data>
    </cmdframe>
    <rspframe>
      <selected />
    </rspframe>
  </device>
</ddb>
