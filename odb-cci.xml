<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="cci" label="IR Helium Compressor Control" ref="cri">
    <comm>
      <device>bbe</device>
      <client>socat - tcp:ltc-i:4002</client>
      <server>socat - udp-l:25105,reuseaddr,fork</server>
    </comm>
    <selection>
      <session id="upd" label="Update Status" ref="upd" />
      <session id="c2on" label="CAS2(S1) ON" ref="x0on" />
      <session id="c2off" label="CAS2(S1) OFF" ref="x0off" />
      <session id="c1on" label="CAS1(S2) ON" ref="x1on" />
      <session id="c1off" label="CAS1(S2) OFF" ref="x1off" />
      <session id="m2on" label="MOIRCS CR(S8) ON" ref="x2on" />
      <session id="m2off" label="MOIRCS CR(S8) OFF" ref="x2off" />
      <session id="m1on" label="MOIRCS MAIN(S9) ON" ref="x3on" />
      <session id="m1off" label="MOIRCS MAIN(S9) OFF" ref="x3off" />
      <session id="cmon" label="CAS MOIRCS/MAIN ON" ref="x0on" />
      <session id="cmoff" label="CAS MOIRCS/MAIN OFF" ref="x0off" />
      <session id="ccon" label="CAS MOIRCS/CR ON" ref="x1on" />
      <session id="ccoff" label="CAS MOIRCS/CR OFF" ref="x1off" />
      <session id="caon" label="CAS BOTH HC ON">
        <statement format="getstat"></statement>
        <statement format="output %d">
          <argv>${field:stat} | 0x0011</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
      <session id="caoff" label="CAS BOTH HC OFF">
        <statement format="getstat"></statement>
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xffee</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
      <session id="maon" label="MOIRCS BOTH ON">
        <statement format="getstat"></statement>
        <statement format="output %d">
          <argv>${field:stat} | 0x1100</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
      <session id="maoff" label="MOIRCS BOTH OFF">
        <statement format="getstat"></statement>
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xeeff</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
    </selection>
    <status>
      <group>
        <var id="s1:sw" label="CAS2(S1)(CIAO) SWITCH" ref="x0:sw" />
        <var id="s1:dr" label="CAS2(S1)(CIAO) DRIVE" ref="x0:dr" />
      </group>
      <group>
        <var id="s1:tm" label="CAS2(S1)(CIAO) TEMP" ref="x0:tm" />
        <var id="s1:pr" label="CAS2(S1)(CIAO) PRESSURE" ref="x0:pr" />
      </group>
      <group>
        <var id="s2:sw" label="CAS1(S2)(MOIRCS/MAIN) SWITCH" ref="x1:sw" />
        <var id="s2:dr" label="CAS1(S2)(MOIRCS/MAIN) DRIVE" ref="x1:dr" />
      </group>
      <group>
        <var id="s2:tm" label="CAS1(S2)(MOIRCS/MAIN) TEMP" ref="x1:tm" />
        <var id="s2:pr" label="CAS1(S2)(MOIRCS/MAIN) PRESSURE" ref="x1:pr" />
      </group>
      <group>
        <var id="s8:sw" label="MOIRCS/CR(S8) SWITCH" ref="x2:sw" />
        <var id="s8:dr" label="MOIRCS/CR(S8) DRIVE" ref="x2:dr" />
      </group>
      <group>
        <var id="s8:tm" label="MOIRCS/CR(S8) TEMP" ref="x2:tm" />
        <var id="s8:pr" label="MOIRCS/CR(S8) PRESSURE" ref="x2:pr" />
      </group>
      <group>
        <var id="s9:sw" label="MOIRCS/MAIN(S9) SWITCH" ref="x3:sw" />
        <var id="s9:dr" label="MOIRCS/MAIN(S9) DRIVE" ref="x3:dr" />
      </group>
      <group>
        <var id="s9:tm" label="MOIRCS/MAIN(S9) TEMP" ref="x3:tm" />
        <var id="s9:pr" label="MOIRCS/MAIN(S9) PRESSURE" ref="x3:pr" />
      </group>
    </status>
  </object>
</odb>
