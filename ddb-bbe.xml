<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb">
  <device id="bbe" label="BB Electric 232SDD16" wait="0.3">
    <serial speed="9600" databit="8" parity="NONE" stopbit="1" flowctl="NONE" />
    <cmdselect>
      <command id="getstat" label="Get Status" response="getstat" type="stat">
        <data>RD</data>
      </command>
      <command id="defio" label="DEFINE IN/OUT" type="config">
        <data>SD</data>
        <data label="msb" encode="bew">0x1111</data>
      </command>
      <command id="output" label="OUTPUT [DDDDD]" type="action">
        <data>SO</data>
        <par encode="bew">
          <rerange>0:65535</rerange>
        </par>
      </command>
    </cmdselect>
    <cmdframe>
      <data label="Prefix">!0</data>
      <selected />
    </cmdframe>
    <rspselect>
      <response id="getstat">
        <field decode="bew">
          <length>2</length>
          <assign>stat</assign>
        </field>
      </response>
    </rspselect>
    <rspframe>
      <selected />
    </rspframe>
  </device>
</ddb>