        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="g0" caption="TAO1 SWITCH"/>
          <group ref="g1" caption="TAO2 SWITCH"/>
        </status>
      </site>
      <site id="cci" dev_id="cci" app_id="bbe" port="53206" label="IR Helium Compressor">
