<?xml version="1.0" encoding="iso-8859-1"?>
<adb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/adb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/adb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="det" label="CsEtr Controller">
    <comm>
      <class>srm</class>
      <client>socat - tcp:ltc-c:4001</client>
      <server>socat - udp-l:25104,reuseaddr,fork</server>
    </comm>
    <command>
      <session id="upd" label="Update Status"></session>
      <session id="c3on" label="CIAX3 POWER ON"></session>
      <session id="c3off" label="CIAX3 POWER OFF"></session>
      <session id="emon" label="CIAX3 EMERGENCY ON"></session>
      <session id="emoff" label="CIAX3 EMERGENCY OFF"></session>
      <session id="ccdon" label="CS CCD ON"></session>
      <session id="ccdoff" label="CS CCD OFF"></session>
      <session id="lanon" label="CS LAN ON"></session>
      <session id="lanoff" label="CS LAN OFF"></session>
      <session id="ipwon" label="CS IPW ON"></session>
      <session id="ipwoff" label="CS IPW OFF"></session>
      <session id="ston" label="STOW PIN ON"></session>
      <session id="stoff" label="STOW PIN OFF"></session>
    </command>
    <status>
      <group label="">
        <title ref="iid" symbol="srm:inst">CAS INST ID</title>
        <title ref="ipw" symbol="off-warn">CAS IPW</title>
      </group>
      <group label="">
        <title ref="stp" symbol="warn">STOW PIN</title>
        <title ref="ccd" symbol="warn">CAS CCD PW</title>
        <title ref="lan" symbol="hide">CAS LAN PW</title>
      </group>
      <group label="">
        <title ref="c3p" symbol="hide">CIAX3 PW</title>
        <title ref="c3e" symbol="alarm">CIAX3 EMG</title>
      </group>
    </status>
  </object>
</adb>