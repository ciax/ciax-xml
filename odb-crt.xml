<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="crt" label="Cart PLC" device="mel" client="nc -u cart3 5000" server="socat - udp-l:25101,reuseaddr,fork">
    <selection>
      <session id="upd" label="Regular status update">
        <command text="getstat" />
        <command text="chken" />
      </session>
      <session id="start_run" label="Running Start">
        <command text="start">
          <par type="text">1</par>
        </command>
      </session>
      <session id="start_jak" label="Jacking Start">
        <command text="start">
          <par type="text">0x100</par>
        </command>
      </session>
      <session id="clear_trg" label="Jacking Start">
        <command text="clear" />
      </session>
      <session id="set_fwd" label="Set Tag to Forward">
        <command text="set_tag">
          <par type="text">98</par>
        </command>
      </session>
      <session id="set_run1" label="Run to Tag 1">
        <command text="set_tag">
          <par type="text">1</par>
        </command>
      </session>
      <session id="set_jak1" label="Jack UP Level 1">
        <command text="set_pos">
          <par type="text">2</par>
        </command>
      </session>
      <session id="pause" label="Pause">
        <command text="stop">
          <par type="text">1</par>
        </command>
      </session>
      <session id="emstop" label="Emergency Stop">
        <command text="stop">
          <par type="text">2</par>
        </command>
      </session>
      <session id="resume" label="Resume from Pause">
        <command text="clear" />
      </session>
    </selection>
    <status>
      <var id="rdy" type="ENUM" label="READY">
        <fields>
          <binary bit="0" ref="x00" />
        </fields>
        <symbol>
          <enum hl="normal" msg="ON">1</enum>
          <enum hl="warn" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="pse" type="ENUM" label="PAUSE">
        <fields>
          <binary bit="1" ref="x00" />
        </fields>
        <symbol>
          <enum hl="alarm" msg="ON">1</enum>
          <enum hl="hide" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="emg" type="ENUM" label="ENERGENCY">
        <fields>
          <binary bit="2" ref="x00" />
        </fields>
        <symbol>
          <enum hl="alarm" msg="ON">1</enum>
          <enum hl="hide" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="stp" type="ENUM" label="STOP">
        <fields>
          <binary bit="3" ref="x00" />
        </fields>
        <symbol>
          <enum hl="alarm" msg="ON">1</enum>
          <enum hl="hide" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="run" type="ENUM" label="RUNNING">
        <fields>
          <binary bit="4" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="RUN">1</enum>
          <enum hl="normal" msg="NO">0</enum>
        </symbol>
      </var>
      <var id="jak" type="ENUM" label="JACKING">
        <fields>
          <binary bit="5" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="JAK">1</enum>
          <enum hl="normal" msg="NO">0</enum>
        </symbol>
      </var>
      <var id="con" type="ENUM" label="CHARGING">
        <fields>
          <binary bit="6" ref="x00" />
        </fields>
        <symbol>
          <enum hl="normal" msg="ON">1</enum>
          <enum hl="alarm" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="flt" type="ENUM" label="Fault">
        <fields>
          <binary bit="7" ref="x00" />
        </fields>
        <symbol>
          <enum hl="alarm" msg="ON">1</enum>
          <enum hl="hide" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="msr" type="ENUM" label="Right Motor Servo ON">
        <fields>
          <binary bit="8" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="msl" type="ENUM" label="Left Motor Servo ON">
        <fields>
          <binary bit="9" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="jd1" type="ENUM" label="Jack Motor1 Servo ON">
        <fields>
          <binary bit="10" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="jd2" type="ENUM" label="Jack Motor2 Servo ON">
        <fields>
          <binary bit="11" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="jd3" type="ENUM" label="Jack Motor3 Servo ON">
        <fields>
          <binary bit="12" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="jd4" type="ENUM" label="Jack Motor4 Servo ON">
        <fields>
          <binary bit="13" ref="x00" />
        </fields>
        <symbol>
          <enum hl="warn" msg="ON">1</enum>
          <enum hl="normal" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="mdar" type="INT" label="Rignt Motor D/A">
        <fields>
          <int ref="x07" signed="true" />
        </fields>
        <symbol>
          <range hl="alarm" msg="LOW">:10</range>
          <range hl="normal" msg="OK">10&lt;:30</range>
          <range hl="alarm" msg="HIGH">30&lt;:</range>
        </symbol>
      </var>
      <var id="mdal" type="INT" label="Left Motor D/A">
        <fields>
          <int ref="x08" signed="true" />
        </fields>
        <symbol>
          <range hl="alarm" msg="LOW">:10</range>
          <range hl="normal" msg="OK">10&lt;:30</range>
          <range hl="alarm" msg="HIGH">30&lt;:</range>
        </symbol>
      </var>
      <var id="bvl" type="FLOAT" label="Battery Voltage">
        <fields>
          <float ref="x16" decimal="2" format="%2.2f" />
        </fields>
        <symbol>
          <range hl="alarm" msg="LOW">:20</range>
          <range hl="warn" msg="LESS">20&lt;:22</range>
          <range hl="normal" msg="OK">22&lt;:26</range>
          <range hl="warn" msg="FULL">26&lt;:28</range>
          <range hl="alarm" msg="HIGH">28&lt;:</range>
        </symbol>
      </var>
      <var id="ist" type="ENUM" label="Instrument">
        <fields>
          <int ref="x17" />
        </fields>
        <symbol>
          <enum hl="normal" msg="MOIRCS">2</enum>
          <enum hl="warn" msg="N/A"></enum>
        </symbol>
      </var>
      <var id="cpos" type="INT" label="Current Position">
        <fields>
          <int ref="x19" />
        </fields>
      </var>
      <var id="dpos" type="INT" label="Destination Position">
        <fields>
          <int ref="x1a" />
        </fields>
      </var>
      <var id="jpos" type="ENUM" label="Jack Position">
        <fields>
          <int ref="x1b" />
        </fields>
        <symbol>
          <enum hl="normal" msg="D">0</enum>
          <enum hl="normal" msg="1(L)">1</enum>
          <enum hl="normal" msg="2(M)">2</enum>
          <enum hl="normal" msg="3(H)">3</enum>
          <enum hl="normal" msg="CHG">4</enum>
          <enum hl="warn" msg="N/A"></enum>
        </symbol>
      </var>
    </status>
  </object>
</odb>