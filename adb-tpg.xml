<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="tpg" id="tpg" label="MOIRCS Pressure">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <group id="g0" caption="Device">
        <command id="upd" label="Update Status">
          <frmcmd name="reqstat" />
          <frmcmd name="getstat" />
        </command>
      </group>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs" table="tpg">
      <group id="g1" caption=""  column="2">
        <string id="st1" label="Turbo Pump Stat" symbol="stat">
          <field ref="st1" />
        </string>
        <float id="ch1" label="Turbo Pump (Torr)" format="%4.2e">
          <field ref="ch1" />
        </float>
        <string id="st2" label="Scroll Pump Stat" symbol="stat">
          <field ref="st2" />
        </string>
        <float id="ch2" label="Scroll Pump (Torr)" format="%4.2e">
          <field ref="ch2" />
        </float>
      </group>
    </status>
  </app>
</adb>
