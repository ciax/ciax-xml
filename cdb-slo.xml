<?xml version='1.0' encoding='ISO-8859-1'?>
<cdb xsi:schemaLocation='http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd' xmlns='http://ciax.sum.naoj.org/ciax-xml/cdb' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
  <class device='slo' id='slo' label='MOIRCS Arm'>
    <commands>
      <session id='upd' label='Update Status'>
        <statement format='bs'/>
        <statement format='p'/>
        <statement format='err'/>
        <repeat from='1' to='5'>
          <statement format='in %d'>
            <argv>$_</argv>
          </statement>
        </repeat>
      </session>
      <session id='reset' label='Reset'>
        <statement format='reset'/>
      </session>
      <session id='defcon' label='Default Config'>
        <statement format='hl 0'/>
        <statement format='e1 0'/>
        <statement format='e2 0'/>
      </session>
      <session id='set0' label='Set 0 Pulses'>
        <statement format='set0'/>
      </session>
    </commands>
    <status>
      <value format='%s' id='bs' label='Moter Moving'>
        <string>bs</string>
      </value>
      <value format='%s' id='con' label='Contact Sensor'>
        <string>in5</string>
      </value>
      <value format='%d' id='err' label='Error Code'>
        <int>err</int>
      </value>
      <value format='%s%s%s%s' id='pos' label='Arm Position'>
        <string>in1</string>
        <string>in2</string>
        <string>in3</string>
        <string>in4</string>
      </value>
      <value format='%f' id='abs' label='ABS position'>
        <float>p</float>
      </value>
    </status>
  </class>
</cdb>
