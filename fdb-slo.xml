<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="slo" label="SLOSYN SS6000i" reference="http://ciax.sum.naoj.org/man/slosyn/slosyn-ss200d6i.pdf" wait="0.1">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <select>
        <command id="getstat" label="Error Status" response="getstat" type="stat">
          <string>err</string>
        </command>
        <command id="bs" label="Running Status" response="bs" type="stat">
          <string>bs</string>
        </command>
        <command id="in" label="Input Status (1-5)" response="in" type="stat">
          <par>1:5</par>
          <string>in</string>
          <string format="(%d)">$1</string>
        </command>
        <command id="p" label="Pulse Number" response="p" type="stat">
          <string>p</string>
        </command>
        <command id="spd" label="Spped Number" response="spd" type="stat">
          <string>spd</string>
        </command>
        <command id="reset" label="Reset" response="default" type="config">
          <string>reset</string>
        </command>
        <command id="hl" label="Hard Limit ON/OFF" response="default" type="config">
          <par>0:1</par>
          <string>hl</string>
          <string>$1</string>
        </command>
        <command id="e1" label="Event1 Enable/Disable" response="default" type="config">
          <par>0:1</par>
          <string>e1=</string>
          <string>$1</string>
        </command>
        <command id="e2" label="Event2 Enable/Disable" response="default" type="config">
          <par>0:1</par>
          <string>e2=</string>
          <string>$1</string>
        </command>
        <command id="set0" label="Set 0 ABS Counter" response="default" type="config">
          <string>abspos=0</string>
        </command>
        <command id="setp" label="Set Pulse Number" response="default" type="config">
          <par>0:999</par>
          <string>p=</string>
          <string format="%5.2f">$1</string>
        </command>
        <command id="setspd" label="Set Speed Number" response="default" type="config">
          <par>0:999</par>
          <string>spd=</string>
          <string format="%5.2f">$1</string>
        </command>
        <command id="stop" label="Stop Running" response="default" type="action">
          <string>stop</string>
        </command>
        <command id="jp" label="JOG Positive" response="default" type="action">
          <string>j=1</string>
        </command>
        <command id="jn" label="JOG Negative" response="default" type="action">
          <string>j=-1</string>
        </command>
        <command id="ma" label="Move Absolute Position" response="default" type="action">
          <par>0:999</par>
          <string>ma=</string>
          <string format="%3.1f">$1</string>
        </command>
        <command id="mi" label="Move Incremental" response="default" type="action">
          <par>-999:999</par>
          <string>mi=</string>
          <string format="%3.1f">$1</string>
        </command>
        <command id="mhn" label="Move To e2 Sensor Negative" response="default" type="action">
          <string>mh=-1</string>
        </command>
      </select>
      <char length="1">0xd</char>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r\n">
      <select>
        <response id="getstat">
          <field assign="err" label="ErrorCode" />
        </response>
        <response id="bs">
          <field assign="bs" label="Moving" />
        </response>
        <response id="p">
          <field assign="p" label="Pulse" />
        </response>
        <response id="spd">
          <field assign="spd" label="Speed" />
        </response>
        <response id="in">
          <array assign="in" label="Input">
            <index range="$1-1" size="5" />
          </array>
        </response>
        <response id="default">
          <field>&gt;</field>
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>