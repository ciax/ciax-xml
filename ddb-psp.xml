<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb">
  <device id="psp" label="Power Supply PSP-603" reference="http://www.bellnw.com/images/man/0685.pdf" wait="0.3">
    <serial speed="2400" databit="8" parity="NONE" stopbit="1" flowctl="NONE" />
    <cmdselect>
      <command id="getstat" label="Get Status" response="getstat" type="stat">
        <data>L</data>
      </command>
      <command id="setvl" type="config" label="Set Voltage Limit [DD]">
        <parameters>
          <par>
            <range>0:20</range>
          </par>
        </parameters>
        <data>SU </data>
        <eval format="%02d">$1</eval>
      </command>
      <command id="setal" type="config" label="Set Current Limit [D.DD]">
        <parameters>
          <par>
            <range>0:&lt;10</range>
          </par>
        </parameters>
        <data>SI </data>
        <eval format="%04.2f">$1</eval>
      </command>
      <command id="setpl" type="config" label="Set Power Limit [DDD]">
        <parameters>
          <par>
            <range>0:&lt;200</range>
          </par>
        </parameters>
        <data>SP </data>
        <eval format="%03d">$1</eval>
      </command>
      <command id="setv" type="config" label="Set Voltage [DD.DD]">
        <parameters>
          <par>
            <range>0:&lt;100</range>
          </par>
        </parameters>
        <data>SV </data>
        <eval format="%05.2f">$1</eval>
      </command>
      <command id="pon" type="action" label="OUTPUT ON">
        <data>KOE</data>
      </command>
      <command id="poff" type="action" label="OUTPUT OFF">
        <data>KOD</data>
      </command>
    </cmdselect>
    <cmdframe>
      <selected />
      <data encode="chr">0xd</data>
    </cmdframe>
    <rspselect>
      <response id="getstat">
        <field label="Format">
          <length>1</length>
          <verify>V</verify>
        </field>
        <field>
          <length>5</length>
          <assign>v</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>A</verify>
        </field>
        <field>
          <length>5</length>
          <assign>a</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>W</verify>
        </field>
        <field>
          <length>5</length>
          <assign>w</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>U</verify>
        </field>
        <field>
          <length>2</length>
          <assign>u</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>I</verify>
        </field>
        <field>
          <length>4</length>
          <assign>i</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>P</verify>
        </field>
        <field>
          <length>3</length>
          <assign>p</assign>
        </field>
        <field label="Format">
          <length>1</length>
          <verify>F</verify>
        </field>
        <field>
          <length>1</length>
          <assign>f1</assign>
        </field>
        <field>
          <length>1</length>
          <assign>f2</assign>
        </field>
        <field>
          <length>1</length>
          <assign>f3</assign>
        </field>
        <field>
          <length>1</length>
          <assign>f4</assign>
        </field>
        <field>
          <length>1</length>
          <assign>f5</assign>
        </field>
        <field>
          <length>1</length>
          <assign>f6</assign>
        </field>
      </response>
    </rspselect>
    <rspframe terminator="\r\n">
      <selected />
    </rspframe>
  </device>
</ddb>