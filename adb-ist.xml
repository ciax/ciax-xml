<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app id="ist" label="Cas Inst DB" version="1">
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <group id="gci" caption="Current Instrument">
        <item id="mcs" label="MCS">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>8</argv>
          </frmcmd>
        </item>
        <item id="comics" label="COMICS">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>2</argv>
          </frmcmd>
        </item>
        <item id="moircs" label="MOIRCS">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>7</argv>
          </frmcmd>
        </item>
        <item id="focas" label="FOCAS">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>4</argv>
          </frmcmd>
        </item>
      </group>
      <group id="gcf" caption="Current Flange">
        <item id="sa" label="SA">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>${sa}</argv>
          </frmcmd>
          <frmcmd name="set">
            <argv>cf</argv>
            <argv>0</argv>
          </frmcmd>
        </item>
        <item id="sb" label="SB">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>${sb}</argv>
          </frmcmd>
          <frmcmd name="set">
            <argv>cf</argv>
            <argv>1</argv>
          </frmcmd>
        </item>
        <item id="sc" label="SC">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>${sc}</argv>
          </frmcmd>
          <frmcmd name="set">
            <argv>cf</argv>
            <argv>2</argv>
          </frmcmd>
        </item>
        <item id="sd" label="SD">
          <frmcmd name="set">
            <argv>cis</argv>
            <argv>${sd}</argv>
          </frmcmd>
          <frmcmd name="set">
            <argv>cf</argv>
            <argv>3</argv>
          </frmcmd>
        </item>
      </group>
    </command>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs">
      <group id="g1" caption="">
        <integer id="cis" label="Current Instrument" symbol="iid_ac"/>
        <integer id="sa" label="Instrument Assigned SA" symbol="iid_ac"/>
        <integer id="sb" label="Instrument Assigned SB" symbol="iid_ac"/>
        <integer id="sc" label="Instrument Assigned SC" symbol="iid_ac"/>
        <integer id="sd" label="Instrument Assigned SD" symbol="iid_ac"/>
        <integer id="cf" label="Current Flange" symbol="flange"/>
      </group>
    </status>
  </app>
</adb>
