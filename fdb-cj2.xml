<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="cj2" version="1" label="OMRON CJ2M-CPU33">
    <stream endian="big" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc" nocache="true">
      <char length="1" label="FINS Header:ICF">0x80</char>
      <char length="1" label="FINS Header:RSV">0x00</char>
      <char length="1" label="FINS Header:GCT">0x02</char>
      <char length="1" label="FINS Header:DNA">0x00</char>
      <char length="1" label="FINS Header:DA1">${ipr}</char>
      <char length="1" label="FINS Header:DA2">0x00</char>
      <char length="1" label="FINS Header:SNA">0x00</char>
      <char length="1" label="FINS Header:SA1">${ipl}</char>
      <char length="1" label="FINS Header:SA2">0x00</char>
      <char length="1" label="FINS Header:SID">0xFF</char>
      <body />
    </cmdframe>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <group id="gst" caption="Status">
        <item id="getstat" label="Get all status" response="getstat" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x01</char>
          <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x0A</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x13</char>
        </item>
        <item id="getexe" label="Get item bit" response="getexe" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x01</char>
          <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x00</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x04</char>
        </item>
        <item id="geterr" label="Get Error Code" response="geterr" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x01</char>
          <char length="1" label="FINS Param:Memory type(D)">0x82</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x00</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x01</char>
        </item>
        <item id="getzlen" label="Get Z Sensor Length" response="getzlen" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x01</char>
          <char length="1" label="FINS Param:Memory type(D)">0x82</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x0A</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x0A</char>
        </item>
      </group>
      <group id="gact" caption="Action">
        <item id="setbit" label="Set Int/Cmd Bit" response="cmdrsp" type="stat">
          <par_num>0:3</par_num>
          <par_num>0:15</par_num>
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x02</char>
          <char length="1" label="FINS Param:Memory type(W)">0x31</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">$1</char>
          <char length="1" label="FINS Param:Bit Number">$2</char>
          <char length="1" label="FINS Param:Bit Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Bit Data Length (L)">0x01</char>
          <char length="1" label="FINS Param:Bit Data">0x01</char>
        </item>
        <item id="clrbit" label="Clear Int/Cmd Bit" response="cmdrsp" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x02</char>
          <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x00</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x04</char>
          <char length="2" label="FINS Param:Bit Data">0x00</char>
          <char length="2" label="FINS Param:Bit Data">0x00</char>
          <char length="2" label="FINS Param:Bit Data">0x00</char>
          <char length="2" label="FINS Param:Bit Data">0x00</char>
        </item>
        <item id="clrint" label="Clear Interrupt Bit" response="cmdrsp" type="stat">
          <char length="1" label="FINS Command:MRC">0x01</char>
          <char length="1" label="FINS Command:SRC">0x02</char>
          <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
          <char length="1" label="FINS Param:Start Address(H)">0x00</char>
          <char length="1" label="FINS Param:Start Address(L)">0x00</char>
          <char length="1" label="FINS Param:Bit Number">0x00</char>
          <char length="1" label="FINS Param:Data Length (H)">0x00</char>
          <char length="1" label="FINS Param:Data Length (L)">0x01</char>
          <char length="2" label="FINS Param:Bit Data">0x00</char>
        </item>
      </group>
    </command>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <char decode="integer" label="FINS Header:ICF" length="1">0xc0</char>
      <char decode="integer" label="FINS Header:RSV" length="1">0x00</char>
      <char decode="integer" label="FINS Header:GCT" length="1">0x02</char>
      <char decode="integer" label="FINS Header:DNA" length="1">0x00</char>
      <char decode="integer" label="FINS Header:DA1" length="1" assign="ipl" />
      <char decode="integer" label="FINS Header:DA2" length="1">0x00</char>
      <char decode="integer" label="FINS Header:SNA" length="1">0x00</char>
      <char decode="integer" label="FINS Header:SA1" length="1" assign="ipr" />
      <char decode="integer" label="FINS Header:SA2" length="1">0x00</char>
      <char decode="integer" label="FINS Header:SID" length="1">0xFF</char>
      <body />
    </rspframe>
    <response xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <item id="getstat">
        <char decode="integer" label="FINS Command:MRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:SRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:MRES" length="1">0x00</char>
        <char decode="integer" label="FINS Command:SRES" length="1">0x00</char>
        <char assign="w" decode="integer" length="2" />
      </item>
      <item id="getexe">
        <char decode="integer" label="FINS Command:MRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:SRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:MRES" length="1">0x00</char>
        <char decode="integer" label="FINS Command:SRES" length="1">0x00</char>
        <char assign="c" decode="integer" length="2" />
      </item>
      <item id="geterr">
        <char decode="integer" label="FINS Command:MRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:SRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:MRES" length="1">0x00</char>
        <char decode="integer" label="FINS Command:SRES" length="1">0x00</char>
        <char assign="err" decode="integer" length="2" />
      </item>
      <item id="getzlen">
        <char decode="integer" label="FINS Command:MRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:SRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:MRES" length="1">0x00</char>
        <char decode="integer" label="FINS Command:SRES" length="1">0x00</char>
        <char assign="d" decode="integer" sign="msb" length="2" />
      </item>
      <item id="cmdrsp" label="command resp">
        <char decode="integer" label="FINS Command:MRC" length="1">0x01</char>
        <char decode="integer" label="FINS Command:SRC" length="1">0x02</char>
        <char decode="integer" label="FINS Command:MRES" length="1">0x00</char>
        <char decode="integer" label="FINS Command:SRES" length="1">0x00</char>
      </item>
      <item id="dmy" label="for csv daemon">
        <char decode="integer" length="1" assign="ipl" />
        <char decode="integer" length="1" assign="ipr" />
        <char decode="integer" length="1" assign="exe" />
      </item>
    </response>
    <field xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbf">
      <array id="w" label="Status">
        <index size="13" />
      </array>
      <array id="c" label="Status">
        <index size="4" />
      </array>
      <array id="d" label="Status">
        <index size="10" />
      </array>
      <assign id="ipl" label="Local IP (CIAX)">1</assign>
      <assign id="ipr" label="Remote IP (DSO)">32</assign>
      <assign id="exe" label="Exe Flag">0</assign>
      <assign id="err" label="FINS Command:Error Code" />
    </field>
  </frame>
</fdb>