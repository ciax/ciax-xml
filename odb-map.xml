<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object device="oma" id="map" label="Compressed Air Pressure at MOIRCS" client="socat - tcp:celaeno:4002" server="socat - udp-l:24102,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <statement>
          <text>getstat</text>
        </statement>
      </session>
    </selection>
    <status>
      <var id="pres" label="Air Pressure (Psi)" symbol="pres">
        <float decimal="2" field="stat" format="%4.2f" />
      </var>
    </status>
    <symbols>
      <symbol id="pres" type="FLOAT">
        <range hl="alarm" msg="HH">110:</range>
        <range hl="warn" msg="H">105:&lt;110</range>
        <range hl="normal" msg="M">95:&lt;105</range>
        <range hl="warn" msg="L">90:&lt;95</range>
        <range hl="alarm" msg="LL">:&lt;90</range>
      </symbol>
    </symbols>
  </object>
</odb>
