        </fields>
        <symbol>
          <enum hl="warn" msg="OFF">1</enum>
          <enum msg="ON" hl="normal">0</enum>
        </symbol>
      </var>
      <var id="1tm" label="[CH1] HC TMP Alarm" type="ENUM">
