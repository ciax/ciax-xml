<?xml version='1.0' encoding='ISO-8859-1'?>
<cdb xsi:schemaLocation='http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd' xmlns='http://ciax.sum.naoj.org/ciax-xml/cdb' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
  <class device='oss' id='oss' label='TSC Control'>
    <commands>
      <session id='upd' label='Update Status'>
        <statement>
          <text>getstat</text>
        </statement>
        <statement>
          <text>iststat</text>
        </statement>
        <statement>
          <text>jakstat</text>
        </statement>
        <statement>
          <text>inrstat</text>
        </statement>
        <statement>
          <text>logstat</text>
        </statement>
        <statement>
          <text>getexit</text>
        </statement>
      </session>
      <session id='login' label='Login'>
        <statement>
          <text>login</text>
        </statement>
      </session>
      <session id='logout' label='Logout'>
        <statement>
          <text>logout</text>
        </statement>
      </session>
      <session id='request' label='Start TCP Status Request'>
        <statement>
          <text>config</text>
          <text>1A1011</text>
        </statement>
      </session>
      <session id='startup' label='Start Up CIAX operation'>
        <statement>
          <text>login</text>
        </statement>
        <statement label='TSC Only Mode(CIAX Mode)'>
          <text>config</text>
          <text>1A1008TSCONLY</text>
        </statement>
      </session>
      <session id='shutdown' label='Shutdown CIAX operation'>
        <statement label='TSC Primaly Mode'>
          <text>config</text>
          <text>1A1008TSCPRI%</text>
        </statement>
        <statement>
          <text>logout</text>
        </statement>
      </session>
      <session id='moircs' label='Set IID MOIRCS'>
        <statement>
          <text>setinst</text>
          <text>12</text>
        </statement>
      </session>
      <session id='focas' label='Set IID FOCAS'>
        <statement>
          <text>setinst</text>
          <text>5</text>
        </statement>
      </session>
      <session id='comics' label='Set IID COMICS'>
        <statement>
          <text>setinst</text>
          <text>7</text>
        </statement>
      </session>
    </commands>
    <status>
      <value format='%d' id='exit' label='Exit Code'>
        <int ref='exit'/>
      </value>
      <value format='%d' id='rts' label='ROT SEL'>
        <int ref='rsel'/>
      </value>
      <value format='%d' id='iid' label='CAS INST ID'>
        <int ref='iid'/>
      </value>
      <value format='%s%s%s%s' id='login' label='Login Status'>
        <string ref='login0'/>
        <string ref='login1'/>
        <string ref='login2'/>
        <string ref='login3'/>
      </value>
      <value format='%d' id='rpw' label='ROT DRIVE'>
        <int ref='rpw'/>
      </value>
      <value format='%s' id='jpw' label='JAK DRIVE'>
        <binary bit='7' ref='bstat'/>
      </value>
      <value format='%s' id='nfb' label='BOLT NFB TRIP'>
        <binary bit='6' ref='bstat'/>
      </value>
      <value format='%s%s' id='rps' label='ROT POS SENSOR'>
        <binary bit='4' ref='bstat'/>
        <binary bit='5' ref='bstat'/>
      </value>
      <value format='%s' id='stw' label='EL STOW'>
        <binary bit='2' ref='stow_1'/>
      </value>
      <value format='%s' id='stp' label='EL 90 STOW'>
        <binary bit='0' ref='stow_1'/>
      </value>
    </status>
  </class>
</cdb>
