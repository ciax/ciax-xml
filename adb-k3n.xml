<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="k3n" id="k3n" label="Helium Compressor Coolant Flow">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <command id="upd" label="Update Status">
        <frmcmd name="getstat" />
      </command>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs" table="k3n">
      <row label="Group">
        <value id="rcode" label="Return Code" symbol="rcode">
          <string ref="rcode" />
        </value>
        <value id="ecode" label="Error Code" symbol="ecode">
          <string ref="ecode" />
        </value>
      </row>
      <row label="Group">
        <value id="flow" label="Helium Compressor Coolant Flow(l/min)" symbol="flow">
          <float format="%4.2f" ref="stat" formula="$#/100" />
        </value>
      </row>
    </status>
  </app>
</adb>
