<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb">
  <device id="oco" label="OMEGA Current Output(D4251)" reference="http://www.omega.com/manuals/manualpdf/M1552.pdf" wait="1">
    <serial speed="300" databit="8" parity="NONE" stopbit="1" flowctl="NONE" />
    <cmdselect>
      <command id="getstat" label="Get Status" response="getstat" type="stat">
        <data>1RD</data>
      </command>
      <command id="i" label="Set Current by argv [DD.DD]" type="action">
        <data>1AO+000</data>
        <par format="%05.2f" />
      </command>
    </cmdselect>
    <cmdframe>
      <data label="Prefix">$</data>
      <selected></selected>
      <data label="Suffix" encode="chr">0xd</data>
    </cmdframe>
    <rspselect>
      <response id="getstat">
        <field>
          <length>9</length>
          <assign>crnt</assign>
        </field>
      </response>
    </rspselect>
    <rspframe>
      <field>
        <length>1</length>
        <verify>*</verify>
      </field>
      <selected />
    </rspframe>
  </device>
</ddb>