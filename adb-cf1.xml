<?xml version="1.0" encoding="iso-8859-1"?>
<adb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/adb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/adb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="cf1" label="S1 Helium Compressor Coolant Flow">
    <comm>
      <class>k3n</class>
      <client>socat - tcp:ltt-o:4001</client>
      <server>socat - udp-l:45601,reuseaddr,fork</server>
    </comm>
    <command>
      <session id="upd" label="Update Status"></session>
    </command>
    <status>
      <group>
        <title ref="rcode" symbol="k3n:rcode">Response Code</title>
        <title ref="ecode" symbol="k3n:ecode">Exit Code</title>
      </group>
      <group>
        <title ref="flow" level="k3n:flow">CS-2(S1) Coolant Flow(l/min)</title>
      </group>
    </status>
  </object>
</adb>