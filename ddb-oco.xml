<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <device id="oco" label="OMEGA Current Output(D4251)" reference="http://www.omega.com/manuals/manualpdf/M1552.pdf" wait="1">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="300" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <data label="Prefix" type="text">$</data>
      <cmdselect>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <data type="text">1RD</data>
        </command>
        <command id="i" label="Set Current by argv [DD.DD]" type="action">
          <parameters>
            <par label="[0-99.99]" validate="range">0:99</par>
          </parameters>
          <data type="text">1AO+000</data>
          <data format="%05.2f" type="formula">$1</data>
        </command>
      </cmdselect>
      <data encode="chr" label="Suffix" type="text">0xd</data>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field length="1">*</field>
      <rspselect>
        <response id="getstat">
          <field assign="crnt" length="9" />
        </response>
      </rspselect>
    </rspframe>
  </device>
</ddb>