<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="mh1" label="MOIRCS Ext Heater 1">
    <comm>
      <device xmlns="">psp</device>
      <client>socat - tcp:merope:59999</client>
      <server>socat - udp-l:24201,reuseaddr,fork</server>
    </comm>
    <selection>
      <session id="upd" label="Update Status">
        <statement format="getstat"></statement>
      </session>
      <session id="setlmt" label="Set Limit 20V/5A/100W">
        <statement format="setvl 20"></statement>
        <statement format="setal 5"></statement>
        <statement format="setpl 100"></statement>
        <statement format="getstat"></statement>
      </session>
      <session id="setv" label="Set Voltage 0-50[V]">
        <parameters>
          <par>
            <range>0:50</range>
          </par>
        </parameters>
        <statement format="setv %f">
          <argv>$1</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
      <session id="setp" label="Set Power 0-100[W]">
        <parameters>
          <par>
            <range>0:100</range>
          </par>
        </parameters>
        <statement format="setv %5.2f">
          <argv>Math.sqrt($1*26)</argv>
        </statement>
        <statement format="getstat"></statement>
      </session>
    </selection>
    <status>
      <group>
        <var id="vol" label="OUTPUT (V)">
          <value format="%3.2f">
            <float field="v" />
          </value>
        </var>
        <var id="amp" label="OUTPUT (A)">
          <value format="%3.2f">
            <float field="a" />
          </value>
        </var>
        <var id="wat" label="OUTPUT (W)">
          <value format="%3.2f">
            <float field="w" />
          </value>
        </var>
      </group>
      <group>
        <var id="vl" label="Limit (V)">
          <value format="%3.2f">
            <float field="u" />
          </value>
        </var>
        <var id="il" label="Limit (A)">
          <value format="%3.2f">
            <float field="i" />
          </value>
        </var>
        <var id="pl" label="Limit (W)">
          <value format="%3.2f">
            <float field="p" />
          </value>
        </var>
      </group>
      <group>
        <var id="sw" label="OUTPUT ON/OFF" symbol="warn">
          <value format="%s">
            <string field="f1" />
          </value>
        </var>
        <var id="tmp" label="OverHeat" symbol="alarm">
          <value format="%s">
            <string field="f2" />
          </value>
        </var>
      </group>
      <group>
        <var id="kn1" label="Wheel Knov Fine" symbol="normal">
          <value format="%s">
            <string field="f3" />
          </value>
        </var>
        <var id="kn2" label="Wheel Knov Unlock" symbol="normal">
          <value format="%s">
            <string field="f4" />
          </value>
        </var>
      </group>
      <group>
        <var id="rem" label="REMOTE" symbol="normal">
          <value format="%s">
            <string field="f5" />
          </value>
        </var>
        <var id="loc" label="Local Lock" symbol="normal">
          <value format="%s">
            <string field="f6" />
          </value>
        </var>
      </group>
    </status>
    <symbols>
      <symbol id="flow" type="FLOAT">
        <enum hl="alarm" msg="HH">
          <range>6:</range>
        </enum>
        <enum hl="warn" msg="H">
          <range>5:&lt;6</range>
        </enum>
        <enum hl="normal" msg="M">
          <range>4:&lt;5</range>
        </enum>
        <enum hl="warn" msg="L">
          <range>3:&lt;4</range>
        </enum>
        <enum hl="alarm" msg="LL">
          <range>:&lt;3</range>
        </enum>
      </symbol>
    </symbols>
  </object>
</odb>