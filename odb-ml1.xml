<?xml version='1.0' encoding='ISO-8859-1'?>
<odb xsi:schemaLocation='http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd' xmlns='http://ciax.sum.naoj.org/ciax-xml/odb' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
  <object id='ml1' label='MOIRCS Pressure Pump'>
    <comm>
      <device>lk8</device>
      <client>socat - tcp:celaeno:4005</client>
      <server>socat - udp-l:24105,reuseaddr,fork</server>
    </comm>
    <selection>
      <session id='upd' label='Update Status'>
        <statement format='getstat'/>
        <statement format='htrstat'/>
        <statement format='spstat'/>
      </session>
    </selection>
    <status>
      <group>
        <var id='ch1' label='MosCold2'>
          <value format='%3.2f'>
            <float>ch1</float>
          </value>
        </var>
        <var id='ch2' label='MosCold1'>
          <value format='%3.2f'>
            <float>ch2</float>
          </value>
        </var>
        <var id='ch3' label='Mask'>
          <value format='%3.2f'>
            <float>ch3</float>
          </value>
        </var>
        <var id='ch4' label='Wheel'>
          <value format='%3.2f'>
            <float>ch4</float>
          </value>
        </var>
      </group>
      <group>
        <var id='ch5' label='D1(NONE)'>
          <value format='%3.2f'>
            <float>ch5</float>
          </value>
        </var>
        <var id='ch6' label='D2(NONE)'>
          <value format='%3.2f'>
            <float>ch6</float>
          </value>
        </var>
        <var id='ch7' label='ChipBox1'>
          <value format='%3.2f'>
            <float>ch7</float>
          </value>
        </var>
        <var id='ch8' label='Det1'>
          <value format='%3.2f'>
            <float>ch8</float>
          </value>
        </var>
      </group>
      <group>
        <var id='sp' label='SetPoint1 (K)'>
          <value format='%3.2f'>
            <float>sp</float>
          </value>
        </var>
        <var id='htr' label='HeaterPower1(%)'>
          <value format='%3.2f'>
            <float>htr</float>
          </value>
        </var>
      </group>
    </status>
  </object>
</odb>
