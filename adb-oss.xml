              <argv>7</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw08" label="EJB Dw 08">
            <frmcmd name="jdw">
              <argv>8</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw09" label="EJB Dw 09">
            <frmcmd name="jdw">
              <argv>9</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw10" label="EJB Dw 10">
            <frmcmd name="jdw">
              <argv>10</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw11" label="EJB Dw 11">
            <frmcmd name="jdw">
