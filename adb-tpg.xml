<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="tpg" id="tpg" label="MOIRCS Pressure">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <cmdset id="upd" label="Update Status">
        <command name="reqstat" />
        <command name="getstat" />
      </cmdset>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs" table="tpg">
      <row>
        <value id="st1" label="Turbo Pump Stat" symbol="stat">
          <string ref="st1" />
        </value>
        <value id="ch1" label="Turbo Pump (Torr)">
          <float format="%4.2e" ref="ch1" />
        </value>
      </row>
      <row>
        <value id="st2" label="Scroll Pump Stat" symbol="stat">
          <string ref="st2" />
        </value>
        <value id="ch2" label="Scroll Pump (Torr)">
          <float format="%4.2e" ref="ch2" />
        </value>
      </row>
    </status>
  </app>
</adb>
