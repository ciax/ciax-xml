<?xml version="1.0" encoding="utf-8"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="mma" label="MOIRCS Arm">
    <comm>
      <class>slo</class>
      <client>socat - tcp:celaeno:4003</client>
      <server>socat - udp-l:24103,reuseaddr,fork</server>
    </comm>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/odbs">
      <group>
        <title ref="bs" symbol="warn" label="Moter Moving" />
        <title ref="con" symbol="normal" label="Contact Sensor" />
        <title ref="err" symbol="slo:err" label="Error Code" />
      </group>
      <group>
        <title ref="pos" symbol="slo:pos" label="Arm Position" />
        <title ref="abs" symbol="slo:pulse" label="ABS position" />
      </group>
    </status>
  </object>
</odb>