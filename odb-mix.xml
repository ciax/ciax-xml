<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:celaeno:4007" device="ixe" id="mix" label="MOIRCS Turret" server="socat - udp-l:45707,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>chkr</text>
          <text>1</text>
        </command>
        <command>
          <text>getp</text>
          <text>1</text>
        </command>
      </session>
    </selection>
    <status>
      <var id="rdy" label="Comm Ready" symbol="ready">
        <binary bit="0" field="stat" />
      </var>
      <var id="srq" label="SRQ" symbol="no">
        <binary bit="1" field="stat" />
      </var>
      <var id="trm" label="Terminal Mode" symbol="no">
        <binary bit="2" field="stat" />
      </var>
      <var id="pme" label="Program Error" symbol="alarm">
        <binary bit="3" field="stat" />
      </var>
      <var id="pse" label="Power Stage Error" symbol="alarm">
        <binary bit="4" field="stat" />
      </var>
      <var id="oes" label="ONE EM Stop" symbol="stop">
        <binary bit="5" field="stat" />
      </var>
      <var id="ees" label="EXT EM Stop" symbol="stop">
        <binary bit="6" field="stat" />
      </var>
      <var id="cmd" label="Computer Mode" symbol="no">
        <binary bit="7" field="stat" />
      </var>
      <var id="r1" label="Running 1">
        <string field="r1" />
      </var>
      <var id="p1" label="Pulse 1">
        <int field="p1" />
      </var>
    </status>
    <symbols>
      <symbol id="stat" type="ENUM">
        <case hl="hide" msg="Measurment data okay">0</case>
        <case hl="warn" msg="Underrange">1</case>
        <case hl="warn" msg="Overrange">2</case>
        <case hl="warn" msg="Sensor error">3</case>
        <case hl="warn" msg="Sensor off">4</case>
        <case hl="warn" msg="No sensor">5</case>
        <case hl="warn" msg="Identification error">6</case>
      </symbol>
    </symbols>
  </object>
</odb>