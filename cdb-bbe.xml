<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="bbe" id="bbe" label="Helium Compressor">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement format="getstat" />
      </session>
      <session id="x0on" label="X0 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x01" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x0off" label="X0 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xfffe" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x1on" label="X1 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x10" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x1off" label="X1 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xffef" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x2on" label="X2 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x100" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x2off" label="X2 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xfeff" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x3on" label="X3 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x1000" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x3off" label="X3 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xefff" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x12on" label="X1 and X2 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x0011" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x12off" label="X1 and X2 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xffee" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x34on" label="X3 and X4 ON">
        <statement format="getstat" />
        <statement format="set stat ${stat}|0x1100" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
      <session id="x34off" label="X3 and X4 OFF">
        <statement format="getstat" />
        <statement format="set stat ${stat}&amp;0xeeff" />
        <statement format="outfld" />
        <statement format="getstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <repeat from="0" to="3">
        <value format="%s" id="xsw$_" label="SWITCH $_" group="$_a">
          <binary bit="$_*4" ref="stat" />
        </value>
        <value format="%s" id="xdr$_" label="DRIVE $_" group="$_a">
          <binary bit="$_*4+1" ref="stat" />
        </value>
        <value format="%s" id="xtm$_" label="TEMP ALARM $_" group="$_b">
          <binary bit="$_*4+2" ref="stat" />
        </value>
        <value format="%s" id="xpr$_" label="PRES ALARM $_" group="$_b">
          <binary bit="$_*4+3" ref="stat" />
        </value>
      </repeat>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <repeat from="0" to="3">
        <enum id="xsw$_" ref="off-warn"></enum>
        <enum id="xdr$_" ref="off-warn"></enum>
        <enum id="xtm$_" ref="alarm"></enum>
        <enum id="xpr$_" ref="alarm"></enum>
      </repeat>
    </symbol>
  </class>
</cdb>
