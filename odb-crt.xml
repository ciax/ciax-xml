<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="crt" label="Cart PLC" device="mel" client="nc -u cart3 5000" server="socat - udp-l:25101,reuseaddr,fork">
    <selection>
      <session id="upd" label="Regular status update">
        <command text="getstat" />
        <command text="chken" />
      </session>
      <session id="start_run" label="Running Start">
        <command text="start">
          <par type="text">1</par>
        </command>
      </session>
      <session id="start_jak" label="Jacking Start">
        <command text="start">
          <par type="text">0x100</par>
        </command>
      </session>
      <session id="clear_trg" label="Jacking Start">
        <command text="clear" />
      </session>
      <session id="set_fwd" label="Set Tag to Forward">
        <command text="set_tag">
          <par type="text">98</par>
        </command>
      </session>
      <session id="set_run1" label="Run to Tag 1">
        <command text="set_tag">
          <par type="text">1</par>
        </command>
      </session>
      <session id="set_jak1" label="Jack UP Level 1">
        <command text="set_pos">
          <par type="text">2</par>
        </command>
      </session>
      <session id="pause" label="Pause">
        <command text="stop">
          <par type="text">1</par>
        </command>
      </session>
      <session id="emstop" label="Emergency Stop">
        <command text="stop">
          <par type="text">2</par>
        </command>
      </session>
      <session id="resume" label="Resume from Pause">
        <command text="clear" />
      </session>
    </selection>
    <status>
      <var id="rdy" label="READY" symbol="non">
        <binary bit="0" ref="x00" />
      </var>
      <var id="pse" label="PAUSE" symbol="alarm">
        <binary bit="1" ref="x00" />
      </var>
      <var id="emg" label="ENERGENCY" symbol="alarm">
        <binary bit="2" ref="x00" />
      </var>
      <var id="stp" label="STOP" symbol="alarm">
        <binary bit="3" ref="x00" />
      </var>
      <var id="run" label="RUNNING" symbol="run">
        <binary bit="4" ref="x00" />
      </var>
      <var id="jak" label="JACKING" symbol="jak">
        <binary bit="5" ref="x00" />
      </var>
      <var id="con" label="CHARGING" symbol="non">
        <binary bit="6" ref="x00" />
      </var>
      <var id="flt" label="Fault" symbol="alarm">
        <binary bit="7" ref="x00" />
      </var>
      <var id="msr" label="Right Motor Servo ON" symbol="noff">
        <binary bit="8" ref="x00" />
      </var>
      <var id="msl" label="Left Motor Servo ON" symbol="noff">
        <binary bit="9" ref="x00" />
      </var>
      <var id="jd1" label="Jack Motor1 Servo ON" symbol="noff">
        <binary bit="10" ref="x00" />
      </var>
      <var id="jd2" label="Jack Motor2 Servo ON" symbol="noff">
        <binary bit="11" ref="x00" />
      </var>
      <var id="jd3" label="Jack Motor3 Servo ON" symbol="noff">
        <binary bit="12" ref="x00" />
      </var>
      <var id="jd4" label="Jack Motor4 Servo ON" symbol="noff">
        <binary bit="13" ref="x00" />
      </var>
      <var id="mdar" label="Rignt Motor D/A" symbol="motor">
        <int ref="x07" signed="true" />
      </var>
      <var id="mdal" label="Left Motor D/A" symbol="motor">
        <int ref="x08" signed="true" />
      </var>
      <var id="bvl" label="Battery Voltage" symbol="battery">
        <float ref="x16" decimal="2" format="%2.2f" />
      </var>
      <var id="ist" label="Instrument" symbol="inst">
        <int ref="x17" />
      </var>
      <var id="cpos" label="Current Position" symbol="int">
        <int ref="x19" />
      </var>
      <var id="dpos" label="Destination Position" symbol="int">
        <int ref="x1a" />
      </var>
      <var id="jpos" label="Jack Position" symbol="jpos">
        <int ref="x1b" />
      </var>
    </status>
    <symbols>
      <symbol type="ENUM" id="non">
        <enum hl="normal" msg="ON">1</enum>
        <enum hl="warn" msg="OFF">0</enum>
      </symbol>
      <symbol type="ENUM" id="noff">
        <enum hl="warn" msg="ON">1</enum>
        <enum hl="normal" msg="OFF">0</enum>
      </symbol>
      <symbol type="ENUM" id="alarm">
        <enum hl="alarm" msg="ON">1</enum>
        <enum hl="hide" msg="OFF">0</enum>
      </symbol>
      <symbol type="ENUM" id="run">
        <enum hl="warn" msg="RUN">1</enum>
        <enum hl="normal" msg="NO">0</enum>
      </symbol>
      <symbol type="ENUM" id="jak">
        <enum hl="warn" msg="JAK">1</enum>
        <enum hl="normal" msg="NO">0</enum>
      </symbol>
      <symbol type="INT" id="motor">
        <range hl="alarm" msg="LOW">:10</range>
        <range hl="normal" msg="OK">10&lt;:30</range>
        <range hl="alarm" msg="HIGH">30&lt;:</range>
      </symbol>
      <symbol type="FLOAT" id="battery">
        <range hl="alarm" msg="LOW">:20</range>
        <range hl="warn" msg="LESS">20&lt;:22</range>
        <range hl="normal" msg="OK">22&lt;:26</range>
        <range hl="warn" msg="FULL">26&lt;:28</range>
        <range hl="alarm" msg="HIGH">28&lt;:</range>
      </symbol>
      <symbol type="ENUM" id="inst">
        <enum hl="normal" msg="MOIRCS">2</enum>
        <enum hl="warn" msg="N/A"></enum>
      </symbol>
      <symbol type="INT" id="int" />
      <symbol type="ENUM" id="jpos">
        <enum hl="normal" msg="D">0</enum>
        <enum hl="normal" msg="1(L)">1</enum>
        <enum hl="normal" msg="2(M)">2</enum>
        <enum hl="normal" msg="3(H)">3</enum>
        <enum hl="normal" msg="CHG">4</enum>
        <enum hl="warn" msg="N/A"></enum>
      </symbol>
    </symbols>
  </object>
</odb>