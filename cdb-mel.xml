<?xml version="1.0" encoding="iso-8859-1"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class id="mel" label="Cart PLC" device="mel">
    <commands>
      <session id="upd" label="Regular status update">
        <statement format="getstat"></statement>
        <statement format="chken"></statement>
      </session>
      <session id="start_run" label="Running Start">
        <statement format="start 1"></statement>
      </session>
      <session id="start_jak" label="Jacking Start">
        <statement format="start 0x100"></statement>
      </session>
      <session id="clear_trg" label="Jacking Start">
        <statement format="clear"></statement>
      </session>
      <session id="set_fwd" label="Set Tag to Forward">
        <statement format="set_tag 98"></statement>
      </session>
      <session id="set_run1" label="Run to Tag 1">
        <statement format="set_tag 1"></statement>
      </session>
      <session id="set_jak1" label="Jack UP Level 1">
        <statement format="set_pos 2"></statement>
      </session>
      <session id="pause" label="Pause">
        <statement format="stop 1"></statement>
      </session>
      <session id="emstop" label="Emergency Stop">
        <statement format="stop 2"></statement>
      </session>
      <session id="resume" label="Resume from Pause">
        <statement format="clear"></statement>
      </session>
    </commands>
    <status>
      <value id="ecode" label="EXIT CODE" format="%04X">
        <int field="ecode" />
      </value>
      <value id="rdy" label="READY" format="%s">
        <binary bit="0" field="x00" />
      </value>
      <value id="pse" label="PAUSE" format="%s">
        <binary bit="1" field="x00" />
      </value>
      <value id="emg" label="ENERGENCY" format="%s">
        <binary bit="2" field="x00" />
      </value>
      <value id="stp" label="STOP" format="%s">
        <binary bit="3" field="x00" />
      </value>
      <value id="run" label="RUNNING" format="%s">
        <binary bit="4" field="x00" />
      </value>
      <value id="jak" label="JACKING" format="%s">
        <binary bit="5" field="x00" />
      </value>
      <value id="con" label="CHARGING" format="%s">
        <binary bit="6" field="x00" />
      </value>
      <value id="flt" label="Fault" format="%s">
        <binary bit="7" field="x00" />
      </value>
      <value id="msr" label="Right Motor Servo" format="%s">
        <binary bit="8" field="x00" inv="true" />
      </value>
      <value id="msl" label="Left Motor Servo" format="%s">
        <binary bit="9" field="x00" inv="true" />
      </value>
      <value id="jd1" label="Jack Motor1 Servo" format="%s">
        <binary bit="10" field="x00" inv="true" />
      </value>
      <value id="jd2" label="Jack Motor2 Servo" format="%s">
        <binary bit="11" field="x00" inv="true" />
      </value>
      <value id="jd3" label="Jack Motor3 Servo" format="%s">
        <binary bit="12" field="x00" inv="true" />
      </value>
      <value id="jd4" label="Jack Motor4 Servo" format="%s">
        <binary bit="13" field="x00" inv="true" />
      </value>
      <value id="mdar" label="Rignt Motor D/A" format="%d">
        <int field="x07" signed="true" />
      </value>
      <value id="mdal" label="Left Motor D/A" format="%d">
        <int field="x08" signed="true" />
      </value>
      <value id="bvl" label="Battery Voltage" format="%2.2f">
        <float decimal="2" field="x16" />
      </value>
      <value id="ist" label="Instrument" format="%d">
        <int field="x17" />
      </value>
      <value id="cpos" label="Current Position" format="%d">
        <int field="x19" />
      </value>
      <value id="dpos" label="Destination Position" format="%d">
        <int field="x1a" />
      </value>
      <value id="jpos" label="Jack Position" format="%d">
        <int field="x1b" />
      </value>
    </status>
  </class>
</cdb>