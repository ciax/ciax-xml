<?xml version="1.0" encoding="shift_jis"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--ReRange-->
  <!--value="^(-?\d+(\.\d+)?&lt;?)?(:&lt;?-?\d+(\.\d+)?)?"-->
  <xsd:simpleType name="rerange">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(&lt;?:?&lt;?[0-9\.]*)+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>