<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:celaeno:4011" device="tpg" id="mt3" label="MOIRCS Pressure Pump" server="socat - udp-l:24111,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <statement format="reqstat"></statement>
        <statement format="getstat"></statement>
      </session>
    </selection>
    <status>
      <var id="st1" label="Turbo Pump Stat" symbol="stat">
        <string field="st1" />
      </var>
      <var id="ch1" label="Turbo Pump (Torr)">
        <float field="ch1" format="%4.2e" />
      </var>
      <var id="st2" label="Scroll Pump Stat" symbol="stat">
        <string field="st1" />
      </var>
      <var id="ch2" label="Scroll Pump (Torr)">
        <float field="ch1" format="%4.2e" />
      </var>
    </status>
    <symbols>
      <symbol id="stat" type="ENUM">
        <case hl="hide" msg="Measurment data okay">0</case>
        <case hl="warn" msg="Underrange">1</case>
        <case hl="warn" msg="Overrange">2</case>
        <case hl="warn" msg="Sensor error">3</case>
        <case hl="warn" msg="Sensor off">4</case>
        <case hl="warn" msg="No sensor">5</case>
        <case hl="warn" msg="Identification error">6</case>
      </symbol>
    </symbols>
  </object>
</odb>