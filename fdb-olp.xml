<?xml version="1.0" encoding="utf-8"?>
<fdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <frame id="olp" label="OMEGA LN2 Pressure Sensor" wait="1">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <data label="Prefix" type="text">*</data>
      <select>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <data type="text">00P1</data>
        </command>
      </select>
      <data encode="chr" label="Suffix" type="text">0xd</data>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r">
      <field label="Prefix" length="1">*</field>
      <select>
        <response id="getstat">
          <field assign="pres" length="6" />
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
