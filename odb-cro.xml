<?xml version="1.0" encoding="utf-8"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="cco" label="OPT Helium Compressor Control" ref="cri">
    <comm>
      <class>bbe</class>
      <client>socat - tcp:ltc-o:4003</client>
      <server>socat - udp-l:25106,reuseaddr,fork</server>
    </comm>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/odbc">
      <alias id="upd" label="Update Status" ref="upd" />
      <alias id="f2on" label="CIAO(S6) ON" ref="x0on" />
      <alias id="f2off" label="CIAO(S6) OFF" ref="x0off" />
      <alias id="m1on" label="COMICS IMG(S4) ON" ref="x2on" />
      <alias id="m1off" label="COMICS IMG(S4) OFF" ref="x2off" />
      <alias id="m2on" label="COMICS SPEC(S5) ON" ref="x3on" />
      <alias id="m2off" label="COMICS SPEC(S5) OFF" ref="x3off" />
      <alias id="maon" label="COMICS BOTH ON" ref="x23on" />
      <alias id="maoff" label="COMICS BOTH OFF" ref="x23off" />
    </command>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/odbs">
      <group>
        <title ref="x0sw" symbol="off-warn" label="CIAO(S6) SWITCH" />
        <title ref="x0dr" symbol="off-warn" label="CIAO(S6) DRIVE" />
      </group>
      <group>
        <title ref="x0tm" symbol="alarm" label="CIAO(S6) TEMP" />
        <title ref="x0pr" symbol="alarm" label="CIAO(S6) PRESSURE" />
      </group>
      <group>
        <title ref="x2sw" symbol="off-warn" label="COMICS IMG(S4) SWITCH" />
        <title ref="x2dr" symbol="off-warn" label="COMICS IMG(S4) DRIVE" />
      </group>
      <group>
        <title ref="x2tm" symbol="alarm" label="COMICS IMG(S4) TEMP" />
        <title ref="x2pr" symbol="alarm" label="COMICS IMG(S4) PRESSURE" />
      </group>
      <group>
        <title ref="x3sw" symbol="off-warn" label="COMICS SPEC(S5) SWITCH" />
        <title ref="x3dr" symbol="off-warn" label="COMICS SPEC(S5) DRIVE" />
      </group>
      <group>
        <title ref="x3tm" symbol="alarm" label="COMICS SPEC(S5) TEMP" />
        <title ref="x3pr" symbol="alarm" label="COMICS SPEC(S5) PRESSURE" />
      </group>
    </status>
  </object>
</odb>