<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="oma" id="oma" label="Compressed Air Pressure at MOIRCS">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="getstat" />
      </session>
    </commands>
    <watch interval="2" xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb">
      <periodic label="Update" period="10">
        <statement command="getstat" />
      </periodic>
    </watch>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <value id="pres" label="Air Pressure (Psi)">
        <float format="%4.2f" decimal="2" ref="stat" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <case id="pres" type="FLOAT">
        <range class="alarm" msg="HH">110:</range>
        <range class="warn" msg="H">105:&lt;110</range>
        <range class="normal" msg="M">95:&lt;105</range>
        <range class="warn" msg="L">90:&lt;95</range>
        <range class="alarm" msg="LL">:&lt;90</range>
      </case>
    </symbol>
  </class>
</cdb>