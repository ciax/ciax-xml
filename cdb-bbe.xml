<?xml version="1.0" encoding="iso-8859-1"?>
<cdb>
  <class id="bbe" label="Helium Compressor" device="bbe">
    <commands>
      <session id="upd" label="Update Status">
        <statement format="getstat" />
      </session>
      <session id="x0on" label="X0 ON">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} | 0x01</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x0off" label="X0 OFF">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xfffe</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x1on" label="X1 ON">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} | 0x10</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x1off" label="X1 OFF">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xffef</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x2on" label="X2 ON">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} | 0x100</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x2off" label="X2 OFF">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xfeff</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x3on" label="X3 ON">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} | 0x1000</argv>
        </statement>
        <statement format="getstat" />
      </session>
      <session id="x3off" label="X3 OFF">
        <statement format="getstat" />
        <statement format="output %d">
          <argv>${field:stat} &amp; 0xefff</argv>
        </statement>
        <statement format="getstat" />
      </session>
    </commands>
    <status>
      <value id="x0sw" label="X0 SWITCH" format="%s">
        <binary bit="0" field="stat" />
      </value>
      <value id="x0dr" label="X0 DRIVE" format="%s">
        <binary bit="1" field="stat" />
      </value>
      <value id="x0tm" label="X0 TEMP ALARM" format="%s">
        <binary bit="2" field="stat" />
      </value>
      <value id="x0pr" label="X0 PRESSURE ALARM" format="%s">
        <binary bit="3" field="stat" />
      </value>
      <value id="x1sw" label="X1 SWITCH" format="%s">
        <binary bit="4" field="stat" />
      </value>
      <value id="x1dr" label="X1 DRIVE" format="%s">
        <binary bit="5" field="stat" />
      </value>
      <value id="x1tm" label="X1 TEMP ALARM" format="%s">
        <binary bit="6" field="stat" />
      </value>
      <value id="x1pr" label="X1 PRESSURE ALARM" format="%s">
        <binary bit="7" field="stat" />
      </value>
      <value id="x2sw" label="X2 SWITCH" format="%s">
        <binary bit="8" field="stat" />
      </value>
      <value id="x2dr" label="X2 DRIVE" format="%s">
        <binary bit="9" field="stat" />
      </value>
      <value id="x2tm" label="X2 TEMP ALARM" format="%s">
        <binary bit="10" field="stat" />
      </value>
      <value id="x2pr" label="X2 PRESSURE ALARM" format="%s">
        <binary bit="11" field="stat" />
      </value>
      <value id="x3sw" label="X3 SWITCH" format="%s">
        <binary bit="12" field="stat" />
      </value>
      <value id="x3dr" label="X3 DRIVE" format="%s">
        <binary bit="13" field="stat" />
      </value>
      <value id="x3tm" label="X3 TEMP ALARM" format="%s">
        <binary bit="14" field="stat" />
      </value>
      <value id="x3pr" label="X3 PRESSURE ALARM" format="%s">
        <binary bit="15" field="stat" />
      </value>
    </status>
  </class>
</cdb>