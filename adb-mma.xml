<?xml version="1.0" encoding="iso-8859-1"?>
<adb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/adb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/adb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="mma" label="MOIRCS Arm">
    <comm>
      <class>slo</class>
      <client>socat - tcp:celaeno:4003</client>
      <server>socat - udp-l:24103,reuseaddr,fork</server>
    </comm>
    <command>
      <session id="upd" label="Update Status"></session>
      <session id="reset" label="Reset"></session>
      <session id="defcon" label="Default Config"></session>
      <session id="set0" label="Set 0 Pulses"></session>
    </command>
    <status>
      <group>
        <title ref="bs" symbol="warn">Moter Moving</title>
        <title ref="con" symbol="normal">Contact Sensor</title>
        <title ref="err" symbol="slo:err">Error Code</title>
      </group>
      <group>
        <title ref="pos" symbol="slo:pos">Arm Position</title>
        <title ref="abs" level="slo:pulse">ABS position</title>
      </group>
    </status>
  </object>
</adb>