<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="lk8" id="lk8" label="MOIRCS LakeShore">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <command id="upd" label="Update Status">
        <frmcmd name="getstat" />
        <frmcmd name="htrstat" />
        <frmcmd name="spstat" />
      </command>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs">
      <group id="g1" caption="" column="4">
        <repeat from="0" to="7">
          <value id="ch%d" label="CH %d" format="%3.2f">
            <float ref="ch" index="$_" />
          </value>
        </repeat>
        <value id="sp" label="SetPoint1 (K)" format="%3.2f">
          <float ref="sp" />
        </value>
        <value id="htr" label="HeaterPower1(percent)" format="%3.2f">
          <float ref="htr" />
        </value>
      </group>
    </status>
  </app>
</adb>