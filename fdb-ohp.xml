<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="ohp" label="OMEGA Helium Pressure Sensor">
    <serial databit="7" flowctl="NONE" parity="ODD" speed="9600" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <body/>
      <char length="1">13</char>
    </cmdframe>
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <command id="getstat" label="Get Status" type="stat">
        <string>*X01</string>
      </command>
      <command id="setd" label="Set default" type="config">
        <string>*W1F14</string>
      </command>
      <command id="init" label="Initialize" type="config">
        <string>*Z02</string>
      </command>
    </commands>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <body/>
    </rspframe>
    <responses xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <response id="getstat">
        <field label="Prefix" length="3">X01</field>
        <field assign="pres" length="5" />
      </response>
      <response id="setd">
        <field label="Command" length="3">W1F</field>
      </response>
      <response id="init">
        <field label="Command" length="3">Z02</field>
      </response>
    </responses>
  </frame>
</fdb>
