<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="ken" label="Kenwood PW-A Series (IF-41RS)" reference="http://ciax.sum.naoj.org/man/kenwood/PWA_m_j.pdf" wait="0.2" ccmethod="sum" maxlength="255">
    <serial databit="7" flowctl="NONE" parity="EVEN" speed="9600" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <char length="1" label="ENQ">5</char>
      <ccrange>
        <select/>
        <char length="1" label="ETX">3</char>
      </ccrange>
      <string format="%02X">${cc}</string>
    </cmdframe>
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <command id="getstat" label="Get status" response="getstat" type="stat">
        <string>AST3</string>
      </command>
      <command id="aon" label="All Output ON" response="default" type="action">
        <string>#SW1</string>
      </command>
      <command id="ack" label="ACK" noaffix="true">
        <char length="1" label="ACK">6</char>
        <string>@</string>
      </command>
    </commands>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\003\r\n">
      <field decode="unsigned" label="ACK" length="1">6</field>
      <select/>
    </rspframe>
    <responses xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <response id="getstat">
        <field assign="stat" label="StatusCode" />
      </response>
      <response id="default" />
    </responses>
  </frame>
</fdb>