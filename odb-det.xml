<?xml version="1.0" encoding="iso-8859-1"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="det" device="srm" label="CsEtr Controller" client="nc ltc-c 4001" server="socat - udp-l:25104,reuseaddr,fork">
    <controls>
      <session id="upd" label="Update Status">
        <command text="getstat" />
      </session>
      <session id="c3on" label="CIAX3 POWER ON">
        <command label="bit0 on" text="fset">
          <par type="text">5</par>
        </command>
      </session>
      <session id="c3off" label="CIAX3 POWER OFF">
        <command label="Bit 0 OFF" text="fset">
          <par type="text">4</par>
        </command>
      </session>
      <session id="emon" label="CIAX3 EMERGENCY ON">
        <command label="Bit 1 ON" text="fset">
          <par type="text">50</par>
        </command>
      </session>
      <session id="emoff" label="CIAX3 EMERGENCY OFF">
        <command label="Bit 1 OFF" text="fset">
          <par type="text">40</par>
        </command>
      </session>
      <session id="ccdon" label="CS CCD ON">
        <command label="Bit 2 ON" text="fset">
          <par type="text">500</par>
        </command>
      </session>
      <session id="ccdoff" label="CS CCD OFF">
        <command label="Bit 2 OFF" text="fset">
          <par type="text">400</par>
        </command>
      </session>
      <session id="lanon" label="CS LAN ON">
        <command label="Bit 3 ON" text="fset">
          <par type="text">5000</par>
        </command>
      </session>
      <session id="lanoff" label="CS LAN OFF">
        <command label="Bit 3 OFF" text="fset">
          <par type="text">4000</par>
        </command>
      </session>
      <session id="ipwon" label="CS IPW ON">
        <command label="Bit 4 ON" text="fset">
          <par type="text">50000</par>
        </command>
      </session>
      <session id="ipwoff" label="CS IPW OFF">
        <command label="Bit 4 OFF" text="fset">
          <par type="text">40000</par>
        </command>
      </session>
      <session id="ston" label="STOW PIN ON">
        <command label="Bit 5 ON" text="fset">
          <par type="text">500000</par>
        </command>
      </session>
      <session id="stoff" label="STOW PIN OFF">
        <command label="Bit 5 OFF" text="fset">
          <par type="text">400000</par>
        </command>
      </session>
    </controls>
    <status>
      <var id="cod" type="ENUM" label="Response Code">
        <fields>
          <string ref="rc" />
        </fields>
      </var>
      <var id="iid" type="ENUM" label="CAS INST ID">
        <fields>
          <string ref="c1d2" />
        </fields>
        <symbol>
          <enum hl="warn" msg="NONE">0</enum>
          <enum hl="normal" msg="CIAX3">1</enum>
          <enum hl="normal" msg="COMICS">2</enum>
          <enum hl="normal" msg="CIAO">3</enum>
          <enum hl="normal" msg="FOCAS">4</enum>
          <enum hl="normal" msg="IRCS">5</enum>
          <enum hl="normal" msg="N/A">6</enum>
          <enum hl="normal" msg="MOIRCS">7</enum>
          <enum hl="normal" msg="MP-CS1">8</enum>
          <enum hl="normal" msg="MP-CS2">9</enum>
        </symbol>
      </var>
      <var id="stp" type="ENUM" label="STOW PIN">
        <fields>
          <binary ref="c1d1" bit="1" />
        </fields>
        <symbol>
          <enum hl="normal" msg="ON">1</enum>
          <enum hl="alarm" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="ipw" type="ENUM" label="CAS IPW">
        <fields>
          <binary ref="c1d1" bit="0" />
        </fields>
        <symbol>
          <enum hl="normal" msg="ON">1</enum>
          <enum hl="alarm" msg="OFF">0</enum>
        </symbol>
      </var>
      <var id="c3p" type="ENUM" label="CIAX3 PW">
        <fields>
          <binary ref="c1d0" bit="0" />
        </fields>
        <symbol>
          <enum hl="hide" msg="OFF">0</enum>
          <enum hl="hide" msg="ON">1</enum>
        </symbol>
      </var>
      <var id="c3e" type="ENUM" label="CIAX3 EMG">
        <fields>
          <binary ref="c1d0" bit="1" />
        </fields>
        <symbol>
          <enum hl="hide" msg="OFF">0</enum>
          <enum hl="alarm" msg="ON">1</enum>
        </symbol>
      </var>
      <var id="ccd" type="ENUM" label="CAS CCD PW">
        <fields>
          <binary ref="c1d0" bit="2" />
        </fields>
        <symbol>
          <enum hl="alarm" msg="ON">1</enum>
        </symbol>
      </var>
      <var id="lan" type="ENUM" label="CAS LAN PW">
        <fields>
          <binary ref="c1d0" bit="3" />
        </fields>
        <symbol>
          <enum hl="hide" msg="OFF">0</enum>
          <enum hl="alarm" msg="ON">1</enum>
        </symbol>
      </var>
    </status>
  </object>
</odb>