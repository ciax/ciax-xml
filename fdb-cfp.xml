<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="cfp" label="Compact Field Point" ccmethod="sum" wait="0">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="115200" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">&gt;</string>
      <ccrange>
        <select>
          <!--String Length must be <32-->
          <command id="getstat" label="Get status 1" response="getstat" type="stat">
            <string>02!J</string>
          </command>
          <command id="getstat2" label="Get status 2" response="getstat2" type="stat">
            <string>03!J</string>
          </command>
          <!---->
          <command id="set" label="Write Discrete [Mask(4)] [Data(4)]" response="default" type="config">
            <string>!L</string>
            <string valid="0:65535" format="%04X">$1</string>
            <string valid="0:65535" format="%04X">$2</string>
          </command>
        </select>
      </ccrange>
      <string format="%02X">${cc}</string>
      <char length="1">13</char>
      <char length="1">10</char>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field label="Prefix" length="1">A</field>
      <ccrange>
        <select>
          <response id="getstat">
            <field assign="stat0" decode="hexstr" label="Status" length="4"></field>
          </response>
          <response id="getstat2">
            <field assign="stat1" decode="hexstr" label="Status" length="4"></field>
          </response>
          <response id="default"></response>
        </select>
      </ccrange>
      <field assign="cc" decode="hexstr" label="BCC" length="2" />
    </rspframe>
  </frame>
</fdb>
