<?xml version="1.0" encoding="utf-8"?>
<odb xmlns="http://ciax.sum.naoj.org/ciax-xml/odb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd">
  <object id="cci" label="IR Helium Compressor Control">
    <comm>
      <class>bbe</class>
      <client>socat - tcp:ltc-i:4002</client>
      <server>socat - udp-l:25105,reuseaddr,fork</server>
    </comm>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/odbc">
      <alias id="upd" label="Update Status" ref="upd" />
      <alias id="c2on" label="CAS2(S1) ON" ref="x0on" />
      <alias id="c2off" label="CAS2(S1) OFF" ref="x0off" />
      <alias id="c1on" label="CAS1(S2) ON" ref="x1on" />
      <alias id="c1off" label="CAS1(S2) OFF" ref="x1off" />
      <alias id="m2on" label="MOIRCS CR(S8) ON" ref="x2on" />
      <alias id="m2off" label="MOIRCS CR(S8) OFF" ref="x2off" />
      <alias id="m1on" label="MOIRCS MAIN(S9) ON" ref="x3on" />
      <alias id="m1off" label="MOIRCS MAIN(S9) OFF" ref="x3off" />
      <alias id="cmon" label="CAS MOIRCS/MAIN ON" ref="x0on" />
      <alias id="cmoff" label="CAS MOIRCS/MAIN OFF" ref="x0off" />
      <alias id="ccon" label="CAS MOIRCS/CR ON" ref="x1on" />
      <alias id="ccoff" label="CAS MOIRCS/CR OFF" ref="x1off" />
      <alias id="caon" label="CAS BOTH HC ON" ref="x12on" />
      <alias id="caoff" label="CAS BOTH HC OFF" ref="x12off" />
      <alias id="maon" label="MOIRCS BOTH ON" ref="x34on" />
      <alias id="maoff" label="MOIRCS BOTH OFF" ref="x34off" />
    </command>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/odbs">
      <title ref="xsw0" symbol="off-warn" label="CAS2(S1)(CIAO) SWITCH" />
      <title ref="xdr0" symbol="off-warn" label="CAS2(S1)(CIAO) DRIVE" />
      <title ref="xtm0" symbol="alarm" label="CAS2(S1)(CIAO) TEMP" />
      <title ref="xpr0" symbol="alarm" label="CAS2(S1)(CIAO) PRESSURE" />
      <title ref="xsw1" symbol="off-warn" label="CAS1(S2)(MOIRCS/MAIN) SWITCH" />
      <title ref="xdr1" symbol="off-warn" label="CAS1(S2)(MOIRCS/MAIN) DRIVE" />
      <title ref="xtm1" symbol="alarm" label="CAS1(S2)(MOIRCS/MAIN) TEMP" />
      <title ref="xpr1" symbol="alarm" label="CAS1(S2)(MOIRCS/MAIN) PRESSURE" />
      <title ref="xsw2" symbol="off-warn" label="MOIRCS/CR(S8) SWITCH" />
      <title ref="xdr2" symbol="off-warn" label="MOIRCS/CR(S8) DRIVE" />
      <title ref="xtm2" symbol="alarm" label="MOIRCS/CR(S8) TEMP" />
      <title ref="xpr2" symbol="alarm" label="MOIRCS/CR(S8) PRESSURE" />
      <title ref="xsw3" symbol="off-warn" label="MOIRCS/MAIN(S9) SWITCH" />
      <title ref="xdr3" symbol="off-warn" label="MOIRCS/MAIN(S9) DRIVE" />
      <title ref="xtm3" symbol="alarm" label="MOIRCS/MAIN(S9) TEMP" />
      <title ref="xpr3" symbol="alarm" label="MOIRCS/MAIN(S9) PRESSURE" />
    </status>
  </object>
</odb>