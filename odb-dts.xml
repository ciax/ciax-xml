<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:cxws:9999" device="oss" id="dts" label="TSC Control" server="socat - udp-l:25105,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>iststat</text>
        </command>
        <command>
          <text>jakstat</text>
        </command>
        <command>
          <text>inrstat</text>
        </command>
        <command>
          <text>logstat</text>
        </command>
        <command>
          <text>getexit</text>
        </command>
      </session>
      <session id="login" label="Login">
        <command label="TSC Login">
          <text>login</text>
        </command>
      </session>
      <session id="logout" label="Logout">
        <command label="TSC Logout">
          <text>logout</text>
        </command>
      </session>
      <session id="request" label="Start TCP Status Request">
        <command>
          <text>config</text>
          <text>1A1011</text>
        </command>
      </session>
      <session id="startup" label="Start Up CIAX operation">
        <command label="TSC Login">
          <text>login</text>
        </command>
        <command label="TSC Only Mode(CIAX Mode)">
          <text>config</text>
          <text>1A1008TSCONLY</text>
        </command>
      </session>
      <session id="shutdown" label="Shutdown CIAX operation">
        <command label="TSC Primaly Mode">
          <text>config</text>
          <text>1A1008TSCPRI%</text>
        </command>
        <command label="TSC Logout">
          <text>logout</text>
        </command>
      </session>
      <session id="moircs" label="Set IID MOIRCS">
        <command>
          <text>setinst</text>
          <text>12</text>
        </command>
      </session>
      <session id="focas" label="Set IID FOCAS">
        <command>
          <text>setinst</text>
          <text>5</text>
        </command>
      </session>
      <session id="comics" label="Set IID COMICS">
        <command>
          <text>setinst</text>
          <text>7</text>
        </command>
      </session>
    </selection>
    <status>
      <var id="exit" label="Exit Code" trail="true">
        <int field="exit" />
      </var>
      <var id="rts" label="ROT SEL" trail="true" symbol="rsel">
        <int field="rsel" />
      </var>
      <var id="iid" label="CAS INST ID" trail="true" symbol="inst">
        <int field="iid" />
      </var>
      <var id="login" label="Login Status" symbol="login">
        <string field="login0" />
        <string field="login1" />
        <string field="login2" />
        <string field="login3" />
      </var>
      <var id="rpw" label="ROT DRIVE" trail="true" symbol="rpw">
        <int field="rpw" />
      </var>
      <var id="jpw" label="JAK DRIVE" trail="true" symbol="normal">
        <binary field="bstat" bit="7" />
      </var>
      <var id="nfb" label="BOLT NFB TRIP" trail="true" symbol="alarm">
        <binary field="bstat" bit="6" />
      </var>
      <var id="rps" label="ROT POS SENSOR" symbol="rps">
        <binary field="bstat" bit="4" />
        <binary field="bstat" bit="5" />
      </var>
    </status>
    <symbols>
      <symbol id="inst" type="ENUM">
        <case hl="warn" msg="NONE">0</case>
        <case hl="normal" msg="IRCS">1</case>
        <case hl="normal" msg="CIAO">3</case>
        <case hl="normal" msg="FOCAS">5</case>
        <case hl="normal" msg="COMICS">7</case>
        <case hl="normal" msg="K3D">10</case>
        <case hl="normal" msg="MOIRCS">12</case>
        <case hl="normal" msg="CIAX3">13</case>
        <case hl="normal" msg="N/A"></case>
      </symbol>
      <symbol id="rsel" type="ENUM">
        <case hl="warn" msg="PFO">1</case>
        <case hl="warn" msg="PFI">2</case>
        <case hl="normal" msg="CAS">4</case>
        <case hl="warn" msg="NSO">8</case>
        <case hl="warn" msg="NSI">10</case>
        <case hl="alarm" msg="N/A"></case>
      </symbol>
      <symbol id="login" type="ENUM">
        <case hl="alarm" msg="OSS Login">OSS</case>
        <case hl="normal" msg="LOGIN">CXWS</case>
        <case hl="warn" msg="LOGOUT"></case>
      </symbol>
      <symbol id="rpw" type="ENUM">
        <case hl="warn" msg="ON">1</case>
        <case hl="normal" msg="OFF">2</case>
      </symbol>
      <symbol id="rps" type="ENUM">
        <case hl="normal" msg="OFF">11</case>
        <case hl="normal" msg="0DEG">10</case>
        <case hl="normal" msg="-4DEG">01</case>
        <case hl="alarm" msg="N/A"></case>
      </symbol>
    </symbols>
  </object>
</odb>