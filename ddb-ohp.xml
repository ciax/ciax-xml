<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <device id="ohp" label="OMEGA Helium Pressure Sensor">
    <serial databit="7" flowctl="NONE" parity="ODD" speed="9600" stopbit="1" />
    <cmdselect>
      <command id="getstat" label="Get Status" type="stat">
        <data type="text">*X01</data>
      </command>
      <command id="setd" label="Set default" type="config">
        <data type="text">*W1F14</data>
      </command>
      <command id="init" label="Initialize" type="config">
        <data type="text">*Z02</data>
      </command>
    </cmdselect>
    <cmdframe>
      <selected />
      <data encode="chr" type="text">13</data>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <rspselect>
        <response id="getstat">
          <field label="Prefix" length="3">X01</field>
          <field assign="pres" length="5" />
        </response>
        <response id="setd">
          <field label="Command" length="3">W1F</field>
        </response>
        <response id="init">
          <field label="Command" length="3">Z02</field>
        </response>
      </rspselect>
    </rspframe>
  </device>
</ddb>
