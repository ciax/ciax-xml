<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_id="oss" id="oss" label="TSC Control" version="6">
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <group id="g0" caption="Device">
        <unit id="upd" label="Status">
          <item id="upd" label="Upd">
            <frmcmd name="logstat"/>
            <frmcmd name="getstat"/>
            <frmcmd name="iststat"/>
            <frmcmd name="jakstat"/>
            <frmcmd name="inrstat"/>
            <frmcmd name="jaklen"/>
            <frmcmd name="zlen"/>
          </item>
        </unit>
        <item id="init" label="Start TCP Status Request">
          <frmcmd name="init"/>
        </item>
        <item id="login" label="Login">
          <frmcmd name="login"/>
          <frmcmd name="logstat"/>
        </item>
        <item id="logout" label="Logout">
          <frmcmd name="logout"/>
          <frmcmd name="logstat"/>
        </item>
        <item id="startup" label="Start Up CIAX operation">
          <frmcmd name="login"/>
          <frmcmd name="tsconly" label="TSC Only Mode(CIAX Mode)"/>
          <frmcmd name="ron"/>
          <frmcmd name="jon"/>
        </item>
        <item id="shutdown" label="Shutdown CIAX operation">
          <frmcmd name="joff"/>
          <frmcmd name="roff"/>
          <frmcmd name="tscpri" label="TSC Primaly Mode"/>
          <frmcmd name="logout"/>
        </item>
      </group>
      <group id="g1" caption="Action">
        <unit id="urot">
          <item id="rhook" label="InR Hook">
            <frmcmd name="rhook"/>
            <frmcmd name="inrstat"/>
          </item>
          <item id="runhk" label="InR UnHook">
            <frmcmd name="runhk"/>
            <frmcmd name="inrstat"/>
          </item>
          <item id="rstop" label="InR Stop">
            <frmcmd name="rstop"/>
            <frmcmd name="inrstat"/>
          </item>
        </unit>
        <unit id="ujak">
          <item id="jup" label="All EJB Up">
            <frmcmd name="ajup"/>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw" label="All EJB Down">
            <frmcmd name="ajdw"/>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jstop" label="All EJB Stop">
            <frmcmd name="ajstop"/>
            <frmcmd name="jakstat"/>
          </item>
        </unit>
        <unit id="ujupind" title="jup[01-16]" label="EJB Up">
          <item id="jup01" label="EJB Up 01">
            <frmcmd name="jup">
              <argv>1</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup02" label="EJB Up 02">
            <frmcmd name="jup">
              <argv>2</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup03" label="EJB Up 03">
            <frmcmd name="jup">
              <argv>3</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup04" label="EJB Up 04">
