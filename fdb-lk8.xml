<?xml version="1.0" encoding="UTF-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd">
  <frame id="lk8" label="Lakeshore MODEL 340 8CH" wait="0.3">
    <serial databit="7" flowctl="NONE" parity="NONE" speed="9600" stopbit="1"/>
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <select>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <code>krdg? c1;</code>
          <code>krdg? c2;</code>
          <code>krdg? c3;</code>
          <code>krdg? c4;</code>
          <code>krdg? d1;</code>
          <code>krdg? d2;</code>
          <code>krdg? d3;</code>
          <code>krdg? d4;</code>
        </command>
        <command id="htrstat" label="Heater Power (%)" response="htrstat" type="stat">
          <code>HTR?</code>
        </command>
        <command id="spstat" label="Set Point (K)" response="spstat" type="stat">
          <code>SETP? 1</code>
        </command>
      </select>
      <code encode="bew" label="Suffix">0x0d0a</code>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" delimiter=";" terminator="\r\n">
      <select>
        <response id="getstat">
          <array assign="ch" label="Value">
            <index range="0:7" size="8"/>
          </array>
        </response>
        <response id="htrstat">
          <field assign="htr" label="Heater Power"/>
        </response>
        <response id="spstat">
          <field assign="sp" label="SetPoint"/>
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
