<?xml version="1.0" encoding="shift_jis"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:include schemaLocation="range.xsd" />
  <!--Command Elements-->
  <xsd:element name="parameters">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="par" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="par">
    <xsd:complexType>
      <xsd:attribute name="label" type="xsd:string" />
      <xsd:attribute name="pattern" type="xsd:string" />
      <xsd:attribute name="range" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
</xsd:schema>