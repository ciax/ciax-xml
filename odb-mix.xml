<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:celaeno:4007" device="ixe" id="mix" label="MOIRCS Turret" server="socat - udp-l:45707,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <command>
          <text>getstat</text>
        </command>
        <repeat range="1:6">
          <command>
            <text>chkr</text>
            <text>${n}</text>
          </command>
          <command>
            <text>getp</text>
            <text>${n}</text>
          </command>
        </repeat>
      </session>
    </selection>
    <status>
      <var id="rdy" label="Comm Ready" symbol="off-warn" trail="true">
        <binary bit="0" field="stat" />
      </var>
      <var id="srq" label="SRQ" symbol="off-warn" trail="true">
        <binary bit="1" field="stat" />
      </var>
      <var id="pme" label="Program Error" symbol="alarm" trail="true">
        <binary bit="3" field="stat" />
      </var>
      <var id="pse" label="Power Stage Error" symbol="alarm" trail="true">
        <binary bit="4" field="stat" />
      </var>
      <var id="oes" label="ONE EM Stop" symbol="alarm" trail="true">
        <binary bit="5" field="stat" />
      </var>
      <var id="ees" label="EXT EM Stop" symbol="alarm">
        <binary bit="6" field="stat" />
      </var>
      <var id="trm" label="Terminal Mode" trail="true" symbol="off-warn">
        <binary bit="2" field="stat" />
      </var>
      <var id="cmd" label="Computer Mode" symbol="off-warn">
        <binary bit="7" field="stat" />
      </var>
      <var id="r1" label="Running 1" trail="true" symbol="normal">
        <string field="r1" />
      </var>
      <var id="p1" label="Pulse 1">
        <int field="p1" />
      </var>
      <var id="r2" label="Running 2" trail="true" symbol="normal">
        <string field="r2" />
      </var>
      <var id="p2" label="Pulse 2">
        <int field="p2" />
      </var>
      <var id="r3" label="Running 3" trail="true" symbol="normal">
        <string field="r3" />
      </var>
      <var id="p3" label="Pulse 3">
        <int field="p3" />
      </var>
      <var id="r4" label="Running 4" trail="true" symbol="normal">
        <string field="r4" />
      </var>
      <var id="p4" label="Pulse 4">
        <int field="p4" />
      </var>
      <var id="r5" label="Running 5" trail="true" symbol="normal">
        <string field="r5" />
      </var>
      <var id="p5" label="Pulse 5">
        <int field="p5" />
      </var>
      <var id="r6" label="Running 6" trail="true" symbol="normal">
        <string field="r6" />
      </var>
      <var id="p6" label="Pulse 6">
        <int field="p6" />
      </var>
    </status>
  </object>
</odb>