<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="oma" id="oma" label="Compressed Air Pressure at MOIRCS">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="getstat" />
      </session>
    </commands>
    <watch interval="2" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbw">
      <event label="Update">
        <periodic period="10" />
        <command>upd</command>
      </event>
    </watch>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <value format="%4.2f" id="pres" label="Air Pressure (Psi)">
        <float decimal="2" ref="stat" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <range id="pres" type="FLOAT">
        <case class="alarm" msg="HH">110:</case>
        <case class="warn" msg="H">105:&lt;110</case>
        <case class="normal" msg="M">95:&lt;105</case>
        <case class="warn" msg="L">90:&lt;95</case>
        <case class="alarm" msg="LL">:&lt;90</case>
      </range>
    </symbol>
  </class>
</cdb>
