<?xml version='1.0' encoding='ISO-8859-1'?>
<ddb xsi:schemaLocation='http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd' xmlns='http://ciax.sum.naoj.org/ciax-xml/ddb' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
  <device id='tpg' label='Pfeiffer Vacuum Gauge TPG262' reference='http://ciax.sum.naoj.org/man/pfeiffer/bg5198be.pdf' wait='0.3'>
    <cmdselect>
      <command id='reqstat' label='Request status' response='reqstat' type='stat'>
        <data label='Measurment data gauges 1&amp;2'>PRX</data>
        <data encode='chr' label='Suffix'>13</data>
      </command>
      <command id='getstat' label='Get status' response='getstat' type='stat'>
        <data encode='chr' label='ENQ'>5</data>
      </command>
    </cmdselect>
    <cmdframe>
      <selected/>
    </cmdframe>
    <rspselect>
      <response id='reqstat'>
        <field decode='chr'>
          <verify>6</verify>
        </field>
      </response>
      <response id='getstat'>
        <field>
          <assign>st1</assign>
        </field>
        <field>
          <assign>ch1</assign>
        </field>
        <field>
          <assign>st2</assign>
        </field>
        <field>
          <assign>ch2</assign>
        </field>
      </response>
    </rspselect>
    <rspframe delimiter=',' terminator='\r\n'>
      <selected/>
    </rspframe>
  </device>
</ddb>
