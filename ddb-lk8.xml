<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml">
  <device id="lk8" label="Lakeshore MODEL 340 8CH" wait="0.3">
    <serial speed="9600" databit="7" parity="NONE" stopbit="1" flowctl="NONE" />
    <selection>
      <session id="getstat" type="stat" label="Get Status">
        <send>
          <data>krdg? c1;</data>
          <data>krdg? c2;</data>
          <data>krdg? c3;</data>
          <data>krdg? c4;</data>
          <data>krdg? d1;</data>
          <data>krdg? d2;</data>
          <data>krdg? d3;</data>
          <data>krdg? d4;</data>
        </send>
        <recv>
          <split delimiter=";">
            <field>ch1</field>
            <field>ch2</field>
            <field>ch3</field>
            <field>ch4</field>
            <field>ch5</field>
            <field>ch6</field>
            <field>ch7</field>
            <field>ch8</field>
          </split>
        </recv>
      </session>
      <session id="htrstat" type="stat" label="Heater Power (%)">
        <send>
          <data>HTR?</data>
        </send>
        <recv>
          <split delimiter=";">
            <field>htr</field>
          </split>
        </recv>
      </session>
      <session id="spstat" type="stat" label="Set Point (K)">
        <send>
          <data>ese?</data>
        </send>
        <recv>
          <split delimiter=";">
            <field>sp</field>
          </split>
        </recv>
      </session>
    </selection>
    <cmdframe>
      <selected />
      <data label="Suffix">\r\n</data>
    </cmdframe>
    <rspframe terminator="\r\n">
      <selected />
    </rspframe>
  </device>
</ddb>