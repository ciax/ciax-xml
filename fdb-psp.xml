<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="psp" label="Power Supply Instek PSP-603" reference="http://www.bellnw.com/images/man/0685.pdf" wait="0.3">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="2400" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <select>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <string>L</string>
        </command>
        <command id="setvl" label="Set Voltage Limit [DD]" type="config">
          <string>SU </string>
          <string valid="0:20" format="%02d">$1</string>
        </command>
        <command id="setal" label="Set Current Limit [D.DD]" type="config">
          <string>SI </string>
          <string format="%04.2f" valid="0:&lt;10">$1</string>
        </command>
        <command id="setpl" label="Set Power Limit [DDD]" type="config">
          <string>SP </string>
          <string format="%03d" valid="0:&lt;200">$1</string>
        </command>
        <command id="setv" label="Set Voltage [DD.DD]" type="config">
          <string>SV </string>
          <string format="%05.2f" valid="0:&lt;100">$1</string>
        </command>
        <command id="pon" label="OUTPUT ON" type="action">
          <string>KOE</string>
        </command>
        <command id="poff" label="OUTPUT OFF" type="action">
          <string>KOD</string>
        </command>
      </select>
      <code length="1">0xd</code>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r\n">
      <select>
        <response id="getstat">
          <field length="1">V</field>
          <field assign="v" label="Output(V)" length="5" />
          <field length="1">A</field>
          <field assign="a" label="Output(A)" length="5" />
          <field length="1">W</field>
          <field assign="w" label="Output(W)" length="5" />
          <field length="1">U</field>
          <field assign="u" label="Limit(V)" length="2" />
          <field length="1">I</field>
          <field assign="i" label="Limit(A)" length="4" />
          <field length="1">P</field>
          <field assign="p" label="Limit(W)" length="3" />
          <field length="1">F</field>
          <field assign="f1" label="Output(ON/OFF)" length="1" />
          <field assign="f2" label="OverHeat" length="1" />
          <field assign="f3" label="ADJ Fine" length="1" />
          <field assign="f4" label="ADJ Unlock" length="1" />
          <field assign="f5" label="Remote" length="1" />
          <field assign="f6" label="Local Lock" length="1" />
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>