<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd">
  <frame id="cj2" label="OMRON CJ2M-CPU33" endian="big">
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <code encode="chr" label="FINS Header:ICF">0x80</code>
      <code encode="chr" label="FINS Header:RSV">0x00</code>
      <code encode="chr" label="FINS Header:GCT">0x02</code>
      <code encode="chr" label="FINS Header:DNA">0x00</code>
      <code encode="chr" label="FINS Header:DA1">${ipr}</code>
      <code encode="chr" label="FINS Header:DA2">0x00</code>
      <code encode="chr" label="FINS Header:SNA">0x00</code>
      <code encode="chr" label="FINS Header:SA1">${ipl}</code>
      <code encode="chr" label="FINS Header:SA2">0x00</code>
      <code encode="chr" label="FINS Header:SID">0xFF</code>
      <select>
        <command id="getstat" label="Get all status" response="getstat" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x01</code>
          <code encode="chr" label="FINS Param:Memory type(W)">0xB1</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)">0x0A</code>
          <code encode="chr" label="FINS Param:Bit Number">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (L)">0x13</code>
        </command>
        <command id="getcmd" label="Get issued command" response="getcmd" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x01</code>
          <code encode="chr" label="FINS Param:Memory type(W)">0xB1</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)">0x00</code>
          <code encode="chr" label="FINS Param:Bit Number">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (L)">0x04</code>
        </command>
        <command id="geterr" label="Get Error Code" response="geterr" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x01</code>
          <code encode="chr" label="FINS Param:Memory type(D)">0x82</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)">0x00</code>
          <code encode="chr" label="FINS Param:Bit Number">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (L)">0x01</code>
        </command>
        <command id="getzlen" label="Get Z Sensor Length" response="getzlen" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x01</code>
          <code encode="chr" label="FINS Param:Memory type(D)">0x82</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)">0x0A</code>
          <code encode="chr" label="FINS Param:Bit Number">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (L)">0x0A</code>
        </command>
        <command id="setbit" label="Set Bit" response="cmdrsp" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x02</code>
          <code encode="chr" label="FINS Param:Memory type(W)">0x31</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)" valid="0:3">$1</code>
          <code encode="chr" label="FINS Param:Bit Number" valid="0:15">$2</code>
          <code encode="chr" label="FINS Param:Bit Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Bit Data Length (L)">0x01</code>
          <code encode="chr" label="FINS Param:Bit Data">0x01</code>
        </command>
        <command id="clrbit" label="Clear All Bit" response="cmdrsp" type="stat">
          <code encode="chr" label="FINS Command:MRC">0x01</code>
          <code encode="chr" label="FINS Command:SRC">0x02</code>
          <code encode="chr" label="FINS Param:Memory type(W)">0xB1</code>
          <code encode="chr" label="FINS Param:Start Address(H)">0x00</code>
          <code encode="chr" label="FINS Param:Start Address(L)">0x00</code>
          <code encode="chr" label="FINS Param:Bit Number">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (H)">0x00</code>
          <code encode="chr" label="FINS Param:Data Length (L)">0x04</code>
          <code encode="bew" label="FINS Param:Bit Data">0x00</code>
          <code encode="bew" label="FINS Param:Bit Data">0x00</code>
          <code encode="bew" label="FINS Param:Bit Data">0x00</code>
          <code encode="bew" label="FINS Param:Bit Data">0x00</code>
        </command>
      </select>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field decode="unsigned" label="FINS Header:ICF" length="1">0xc0</field>
      <field decode="unsigned" label="FINS Header:RSV" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:GCT" length="1">0x02</field>
      <field decode="unsigned" label="FINS Header:DNA" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:DA1" length="1" assign="ipl" />
      <field decode="unsigned" label="FINS Header:DA2" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SNA" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SA1" length="1" assign="ipr" />
      <field decode="unsigned" label="FINS Header:SA2" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SID" length="1">0xFF</field>
      <select>
        <response id="getstat">
          <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
          <array assign="w" decode="unsigned" label="Status" length="2">
            <index range="0:12" size="13" />
          </array>
        </response>
        <response id="getcmd">
          <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
          <array assign="c" decode="unsigned" label="Status" length="2">
            <index range="0:3" size="4" />
          </array>
        </response>
        <response id="geterr">
          <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
          <field assign="err" decode="unsigned" label="FINS Command:Error Code" length="2">0</field>
        </response>
        <response id="getzlen">
          <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
          <array assign="d" decode="unsigned" label="Status" length="2">
            <index range="0:9" size="8" />
          </array>
        </response>
        <response id="cmdrsp" label="command resp">
          <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
          <field decode="unsigned" label="FINS Command:SRC" length="1">0x02</field>
          <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
          <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
        </response>
        <response id="dmy" label="for csv daemon">
          <field decode="unsigned" label="Local IP (CIAX)" length="1" assign="ipl">1</field>
          <field decode="unsigned" label="Remote IP (DSO)" length="1" assign="ipr">32</field>
          <field decode="unsigned" label="Exe Flag" length="1" assign="exe">0</field>
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
