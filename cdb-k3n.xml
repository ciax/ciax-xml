<?xml version="1.0" encoding="iso-8859-1"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="k3n" id="k3n" label="Helium Compressor Coolant Flow">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement format="getstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <value format="%s%s" id="err" label="STAT">
        <string ref="rcode" />
        <string ref="ecode" />
      </value>
      <value format="%4.2f" id="flow" label="Helium Compressor Coolant Flow(l/min)">
        <float decimal="2" ref="stat" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <regexp id="err">
        <case class="hide" msg="OK">000000</case>
        <case class="alarm" msg="Error">.</case>
      </regexp>
      <range id="flow" type="FLOAT">
        <case class="alarm" msg="HH">6:</case>
        <case class="warn" msg="H">5:&lt;6</case>
        <case class="normal" msg="M">4:&lt;5</case>
        <case class="warn" msg="L">3:&lt;4</case>
        <case class="alarm" msg="LL">:&lt;3</case>
      </range>
    </symbol>
  </class>
</cdb>