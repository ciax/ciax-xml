<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="fpa" label="Field Point Analog Input(FP-AI-100)" reference="http://ciax.sum.naoj.org/man/fieldpoint/fp-1000prm-321632c.pdf" ccmethod="sum" wait="0.01">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="38400" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">&gt;01</string>
      <body/>
      <string>??</string>
      <char length="1">13</char>
      <char length="1">10</char>
    </cmdframe>
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <!--String Length must be <32-->
      <item id="getstat" label="Get status" response="getstat" type="stat">
        <string>!F00FF</string>
      </item>
      <item id="setr1" label="Set range +-1.2V" response="default" type="config">
        <string>!D00FF</string>
        <repeat from="1" to="8">
          <string>0000107</string>
        </repeat>
      </item>
      <item id="setr6" label="Set range +-6V" response="default" type="config">
        <string>!D00FF</string>
        <repeat from="1" to="8">
          <string>0000105</string>
        </repeat>
      </item>
      <item id="setr15" label="Set range +-15V" response="default" type="config">
        <string>!D00FF</string>
        <repeat from="1" to="8">
          <string>0000112</string>
        </repeat>
      </item>
    </commands>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr" terminator="\r">
      <field label="Prefix">A</field>
      <ccrange>
        <body/>
      </ccrange>
      <field assign="cc" decode="hexstr" label="BCC" length="2" />
    </rspframe>
    <responses xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <response id="getstat">
        <array assign="stat" decode="hexstr" label="Status" length="4">
          <index size="8" />
        </array>
      </response>
      <response id="default" />
    </responses>
  </frame>
</fdb>
