<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="olp" label="OMEGA LN2 Pressure Sensor">
    <stream wait="1" terminator="\r"/>
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1"/>
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">*</string>
      <body/>
      <char length="1" label="Suffix">0xd</char>
    </cmdframe>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <group id="gst" caption="Status">
        <item id="getstat" label="Get Status" response="getstat" type="stat">
          <string>00P1</string>
        </item>
      </group>
    </command>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <string label="Prefix">*</string>
      <body/>
    </rspframe>
    <response xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <item id="getstat">
        <string assign="pres" length="6"/>
      </item>
    </response>
    <field xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <var id="pres"/>
    </field>
  </frame>
</fdb>
