<?xml version="1.0" encoding="utf-8"?>
<cdb xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb">
  <class frame="tpg" id="tpg" label="MOIRCS Pressure">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="reqstat" />
        <statement command="getstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <row>
        <value id="st1" label="Turbo Pump Stat" symbol="stat">
          <string ref="st1" />
        </value>
        <value id="ch1" label="Turbo Pump (Torr)">
          <float format="%4.2e" ref="ch1" />
        </value>
      </row>
      <row>
        <value id="st2" label="Scroll Pump Stat" symbol="stat">
          <string ref="st2" />
        </value>
        <value id="ch2" label="Scroll Pump (Torr)">
          <float format="%4.2e" ref="ch2" />
        </value>
      </row>
    </status>
  </class>
  <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
    <table id="stat" type="string">
      <case class="hide" msg="Measurment data okay">0</case>
      <case class="warn" msg="Underrange">1</case>
      <case class="warn" msg="Overrange">2</case>
      <case class="warn" msg="Sensor error">3</case>
      <case class="warn" msg="Sensor off">4</case>
      <case class="warn" msg="No sensor">5</case>
      <case class="warn" msg="Identification error">6</case>
    </table>
  </symbol>
</cdb>