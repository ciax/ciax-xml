<?xml version="1.0" encoding="iso-8859-1"?>
<adb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/adb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/adb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="mh1" label="MOIRCS Ext Heater 1">
    <comm>
      <class>psp</class>
      <client>socat - tcp:merope:59999</client>
      <server>socat - udp-l:24201,reuseaddr,fork</server>
    </comm>
    <command>
      <session id="upd" label="Update Status"></session>
      <session id="setlmt" label="Set Limit 20V/5A/100W"></session>
      <session id="setv" label="Set Voltage 0-50[V]"></session>
      <session id="setp" label="Set Power 0-100[W]"></session>
    </command>
    <status>
      <group>
        <title ref="vol">OUTPUT (V)</title>
        <title ref="amp">OUTPUT (A)</title>
        <title ref="wat">OUTPUT (W)</title>
      </group>
      <group>
        <title ref="vl">Limit (V)</title>
        <title ref="il">Limit (A)</title>
        <title ref="pl">Limit (W)</title>
      </group>
      <group>
        <title ref="sw" symbol="warn">OUTPUT ON/OFF</title>
        <title ref="tmp" symbol="alarm">OverHeat</title>
      </group>
      <group>
        <title ref="kn1" symbol="normal">Wheel Knov Fine</title>
        <title ref="kn2" symbol="normal">Wheel Knov Unlock</title>
      </group>
      <group>
        <title ref="rem" symbol="normal">REMOTE</title>
        <title ref="loc" symbol="normal">Local Lock</title>
      </group>
    </status>
  </object>
</adb>