<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <device id="bbe" label="BB Electric 232SDD16" wait="0.3">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="9600" stopbit="1" />
    <cmdselect>
      <command id="getstat" label="Get Status" response="getstat" type="stat">
        <data text="RD" type="text" />
      </command>
      <command id="output" label="OUTPUT [DDDDD]" type="action">
        <parameters>
          <par label="[0-65535]" range="0:65535"></par>
        </parameters>
        <data text="SO" type="text" />
        <data encode="bew" text="$1" type="formula" />
      </command>
      <command id="outfld" label="OUTPUT from Field" nocache="true" type="action">
        <data text="SO" type="text" />
        <data encode="bew" text="${stat}" type="formula" />
      </command>
    </cmdselect>
    <cmdframe>
      <data label="Prefix" text="!0" type="text" />
      <selected />
    </cmdframe>
    <rspselect>
      <response id="getstat">
        <field assign="stat" decode="bew" />
      </response>
    </rspselect>
    <rspframe>
      <selected />
    </rspframe>
  </device>
</ddb>