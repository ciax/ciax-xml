<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="oco" label="OMEGA Current Output(D4251)" reference="http://www.omega.com/manuals/manualpdf/M1552.pdf" wait="1">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="300" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <string label="Prefix">$</string>
      <select>
        <command id="getstat" label="Get Status" response="getstat" type="stat">
          <string>1RD</string>
        </command>
        <command id="i" label="Set Current by argv [DD.DD]" type="action">
          <par_num>0:99</par_num>
          <string>1AO+000</string>
          <string format="%05.2f">$1</string>
        </command>
      </select>
      <char length="1" label="Suffix">0xd</char>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field length="1">*</field>
      <select>
        <response id="getstat">
          <field assign="crnt" length="9" />
        </response>
      </select>
    </rspframe>
  </frame>
</fdb>
