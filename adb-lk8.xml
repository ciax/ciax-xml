<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="lk8" id="lk8" label="MOIRCS LakeShore">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <command id="upd" label="Update Status">
        <frmcmd name="getstat" />
        <frmcmd name="htrstat" />
        <frmcmd name="spstat" />
      </command>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs">
      <row label="Group">
        <repeat from="0" to="3">
          <value id="ch%d" label="CH %d">
            <float ref="ch" index="$_" format="%3.2f" />
          </value>
        </repeat>
      </row>
      <row label="Group">
        <repeat from="4" to="7">
          <value id="ch%d" label="CH %d">
            <float ref="ch" index="$_" format="%3.2f" />
          </value>
        </repeat>
      </row>
      <row label="Group">
        <value id="sp" label="SetPoint1 (K)">
          <float ref="sp" format="%3.2f" />
        </value>
        <value id="htr" label="HeaterPower1(percent)">
          <float ref="htr" format="%3.2f" />
        </value>
      </row>
    </status>
  </app>
</adb>