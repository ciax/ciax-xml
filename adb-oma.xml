<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_type="oma" id="oma" label="Compressed Air Pressure at MOIRCS">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <cmdset id="upd" label="Update Status">
        <command name="getstat" />
      </cmdset>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs" table="oma">
      <row label="Group">
        <value id="pres" label="Air Pressure (Psi)" symbol="pres">
          <float format="%4.2f" ref="stat" />
        </value>
      </row>
    </status>
    <watch interval="2" xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb">
      <periodic label="Update" period="10">
        <command name="getstat" />
      </periodic>
    </watch>
  </app>
</adb>
