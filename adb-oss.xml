              <argv>4</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup05" label="EJB Up 05">
            <frmcmd name="jup">
              <argv>5</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup06" label="EJB Up 06">
            <frmcmd name="jup">
              <argv>6</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup07" label="EJB Up 07">
            <frmcmd name="jup">
              <argv>7</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup08" label="EJB Up 08">
            <frmcmd name="jup">
              <argv>8</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup09" label="EJB Up 09">
            <frmcmd name="jup">
              <argv>9</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup10" label="EJB Up 10">
            <frmcmd name="jup">
              <argv>10</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup11" label="EJB Up 11">
            <frmcmd name="jup">
              <argv>11</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup12" label="EJB Up 12">
            <frmcmd name="jup">
              <argv>12</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup13" label="EJB Up 13">
            <frmcmd name="jup">
              <argv>13</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup14" label="EJB Up 14">
            <frmcmd name="jup">
              <argv>14</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup15" label="EJB Up 15">
            <frmcmd name="jup">
              <argv>15</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jup16" label="EJB Up 16">
            <frmcmd name="jup">
              <argv>16</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
        </unit>
        <unit id="ujdwind" title="jdw[01-16]" label="EJB Down">
          <item id="jdw01" label="EJB Dw 01">
            <frmcmd name="jdw">
              <argv>1</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw02" label="EJB Dw 02">
            <frmcmd name="jdw">
              <argv>2</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw03" label="EJB Dw 03">
            <frmcmd name="jdw">
              <argv>3</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw04" label="EJB Dw 04">
            <frmcmd name="jdw">
              <argv>4</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw05" label="EJB Dw 05">
            <frmcmd name="jdw">
              <argv>5</argv>
            </frmcmd>
            <frmcmd name="jakstat"/>
          </item>
          <item id="jdw06" label="EJB Dw 06">
            <frmcmd name="jdw">
              <argv>6</argv>
            </frmcmd>
