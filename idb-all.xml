<?xml version="1.0" encoding="utf-8"?>
<idb xmlns="http://ciax.sum.naoj.org/ciax-xml/idb">
  <project id="ciax" caption="CIAX" include="cmp" column="2">
    <group id="sum" caption="CIAX Summit">
      <instance id="crt" app_id="mel" host="cxws2" port="25101" label="CIAX CART 3"/>
      <instance id="dsi" app_id="cj2" host="cxws2" port="25102" label="IR Stand-by"/>
      <instance id="dso" app_id="cj2" host="cxws2" port="25103" label="OPT Stand-by"/>
      <instance id="det" app_id="srm" host="cxws2" port="25104" label="CsEtr Control"/>
      <instance id="dts" app_id="oss" host="cxws2" port="25105" label="TSC Control"/>
    </group>
    <group id="sim" caption="SimLab">
      <instance id="cr2" app_id="mel" host="ciax" port="25502" label="CIAX CART 2"/>
    </group>
    <group id="cfg" caption="Setting">
      <instance id="c3j" app_id="cjk" host="ciax" port="25501" frm_site="crt" label="CART 3 Jack Setting">
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="id0t0" caption="COMICS/CAS" />
          <group ref="id0t1" caption="COMICS/OPT/MOV" />
          <group ref="id1t0" caption="CIAO/CAS" />
          <group ref="id1t2" caption="CIAO/OPT/FIX" />
          <group ref="id2t0" caption="FOCAS/CAS" />
          <group ref="id2t4" caption="FOCAS/IR/FIX" />
          <group ref="id3t0" caption="MOIRCS/CAS" />
          <group ref="id3t3" caption="MOIRCS/IR/MOV" />
          <group ref="id4t0" caption="KYOTO3D/CAS" />
        </status>
      </instance>
      <instance id="c2j" app_id="cjk" host="ciax" port="25503" frm_site="cr2" label="CART 2 Jack Setting"/>
    </group>
  </project>
  <project id="cmp" caption="Helium Compressor">
    <group id="sum" caption="Summit Helium Compressor">
      <instance id="cro" app_id="bbe" host="ciax" port="25106" label="OPT Helium Compressor">
        <alias xmlns="http://ciax.sum.naoj.org/ciax-xml/idbc">
          <item id="on" label="Alias for X0 ON" ref="x0on" />
          <item id="off" label="Alias for X0 OFF" ref="x0off" />
        </alias>
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="g0" caption="X0-3" />
          <group ref="g1" caption="X4-7" />
          <group ref="g2" caption="X8-11" />
          <group ref="g3" caption="X12-15" />
        </status>
        <watch xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb" interval="0.2">
          <regular period="60"/>
          <event label="ON Executed 1" id="on1">
            <onchange var="xsw0"/>
            <pattern var="xsw0">1</pattern>
            <pattern var="xsw1">0</pattern>
            <exec name="x1on"/>
          </event>
          <event label="ON Executed 2" id="on2">
            <onchange var="xsw1"/>
            <pattern var="xsw1">1</pattern>
            <pattern var="xsw2">0</pattern>
            <exec name="x2on"/>
          </event>
          <event label="ON Executed 3" id="on3">
            <onchange var="xsw2"/>
            <pattern var="xsw2">1</pattern>
            <pattern var="xsw3">0</pattern>
            <exec name="x3on"/>
          </event>
          <event label="OFF Executed 1" id="off1">
            <onchange var="xsw0"/>
            <pattern var="xsw0">0</pattern>
            <pattern var="xsw1">1</pattern>
            <exec name="x1off"/>
          </event>
          <event label="OFF Executed 2" id="off2">
            <onchange var="xsw1"/>
            <pattern var="xsw1">0</pattern>
            <pattern var="xsw2">1</pattern>
            <exec name="x2off"/>
          </event>
          <event label="OFF Executed 3" id="off3">
            <onchange var="xsw2"/>
            <pattern var="xsw2">0</pattern>
            <pattern var="xsw3">1</pattern>
            <exec name="x3off"/>
          </event>
        </watch>
      </instance>
      <instance id="cci" app_id="bbe" host="ciax" port="25108" label="IR Helium Compressor">
        <alias xmlns="http://ciax.sum.naoj.org/ciax-xml/idbc">
          <item id="c2on" label="CAS2(S1) ON" ref="x0on" />
          <item id="c2off" label="CAS2(S1) OFF" ref="x0off" />
          <item id="c1on" label="CAS1(S2) ON" ref="x1on" />
          <item id="c1off" label="CAS1(S2) OFF" ref="x1off" />
          <item id="m2on" label="MOIRCS CR(S8) ON" ref="x2on" />
          <item id="m2off" label="MOIRCS CR(S8) OFF" ref="x2off" />
          <item id="m1on" label="MOIRCS MAIN(S9) ON" ref="x3on" />
          <item id="m1off" label="MOIRCS MAIN(S9) OFF" ref="x3off" />
          <item id="cmon" label="CAS MOIRCS/MAIN ON" ref="x0on" />
          <item id="cmoff" label="CAS MOIRCS/MAIN OFF" ref="x0off" />
          <item id="ccon" label="CAS MOIRCS/CR ON" ref="x1on" />
          <item id="ccoff" label="CAS MOIRCS/CR OFF" ref="x1off" />
          <item id="caon" label="CAS BOTH HC ON" ref="x01on" />
          <item id="caoff" label="CAS BOTH HC OFF" ref="x01off" />
          <item id="maon" label="MOIRCS BOTH ON" ref="x23on" />
          <item id="maoff" label="MOIRCS BOTH OFF" ref="x23off" />
        </alias>
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="g0" caption="CAS2(S1)(CIAO)" />
          <group ref="g1" caption="CAS1(S2)(MOIRCS/MAIN)" />
          <group ref="g2" caption="MOIRCS/CR(S8)" />
          <group ref="g3" caption="MOIRCS/MAIN(S9)" />
        </status>
      </instance>
      <instance id="cco" app_id="bbe" host="ciax" port="25109" label="OPT Helium Compressor">
        <alias xmlns="http://ciax.sum.naoj.org/ciax-xml/idbc">
          <item id="f2on" label="CIAO(S6) ON" ref="x0on" />
          <item id="f2off" label="CIAO(S6) OFF" ref="x0off" />
          <item id="m1on" label="COMICS IMG(S4) ON" ref="x2on" />
          <item id="m1off" label="COMICS IMG(S4) OFF" ref="x2off" />
          <item id="m2on" label="COMICS SPEC(S5) ON" ref="x3on" />
          <item id="m2off" label="COMICS SPEC(S5) OFF" ref="x3off" />
          <item id="maon" label="COMICS BOTH ON" ref="x23on" />
          <item id="maoff" label="COMICS BOTH OFF" ref="x23off" />
        </alias>
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="g0" caption="CIAO(S6) SWITCH" />
          <group ref="g2" caption="COMICS IMG(S4)" />
          <group ref="g3" caption="COMICS SPEC(S5)" />
        </status>
      </instance>
      <instance id="cri" app_id="bbe" host="ciax" port="25110" label="IR Helium Compressor">
        <alias xmlns="http://ciax.sum.naoj.org/ciax-xml/idbc">
          <item id="f1on" label="FMOS/IRS1(S10)ON" ref="x0on" />
          <item id="f1off" label="FMOS/IRS1(S10)OFF" ref="x0off" />
          <item id="f2on" label="FMOS/IRS2(S11)ON" ref="x2on" />
          <item id="f2off" label="FMOS/IRS2(S11)OFF" ref="x2off" />
        </alias>
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <group ref="g0" caption="FMOS/IRS1(S10)" />
          <group ref="g2" caption="FMOS/IRS2(S11)" />
        </status>
      </instance>
      <instance id="cfa" app_id="k3n" host="ciax" port="25210" label="S10 HC Coolant Flow">
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <index ref="flow" label="FMOS-K(S10) Coolant Flow" />
        </status>
      </instance>
      <instance id="cf1" app_id="k3n" host="ciax" port="25201" label="S1 HC Coolant Flow">
        <status xmlns="http://ciax.sum.naoj.org/ciax-xml/idbs">
          <index ref="flow" label="CS-2(S1) Coolant Flow(l/min)" />
        </status>
      </instance>
    </group>
  </project>
  <project id="dmy" caption="Dummy simulator">
    <group id="dmy" caption="Dummy simulator">
      <instance id="tsl" app_id="slo" frm_site="tsl" host="localhost" port="25699" label="TEST Slosyn"/>
      <instance id="tfp" app_id="cfp" frm_site="tfp" host="localhost" port="25698" label="TEST Field Point"/>
    </group>
  </project>
</idb>
