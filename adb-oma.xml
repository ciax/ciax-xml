<?xml version="1.0" encoding="utf-8"?>
<adb xmlns="http://ciax.sum.naoj.org/ciax-xml/adb">
  <app frm_id="oma" id="oma" label="Compressed Air Pressure at MOIRCS">
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/adbc">
      <group id="g0" caption="Device">
        <item id="upd" label="Update Status">
          <frmcmd name="getstat" />
        </item>
      </group>
    </command>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/adbs" symtbl="oma">
      <group id="g1" caption="">
        <float id="pres" label="Air Pressure (Psi)" symbol="pres" format="%4.2f">
          <field ref="stat" />
        </float>
      </group>
    </status>
    <watch xmlns="http://ciax.sum.naoj.org/ciax-xml/wdb" interval="2">
      <regular period="60"/>
    </watch>
  </app>
</adb>
