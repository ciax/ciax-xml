<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="lk8" id="lk8" label="MOIRCS LakeShore">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="getstat" />
        <statement command="htrstat" />
        <statement command="spstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <repeat from="0" to="3">
        <value id="ch%d" label="CH %d" group="a">
          <float ref="ch:$_" format="%3.2f" />
        </value>
      </repeat>
      <repeat from="4" to="7">
        <value id="ch%d" label="CH %d" group="b">
          <float ref="ch:$_" format="%3.2f" />
        </value>
      </repeat>
      <value id="sp" label="SetPoint1 (K)" group="s">
        <float ref="sp" format="%3.2f" />
      </value>
      <value id="htr" label="HeaterPower1(%)" group="s">
        <float ref="htr" format="%3.2f" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <repeat from="0" to="7">
        <case id="ch%d" />
      </repeat>
      <case id="sp" />
      <case id="htr" />
    </symbol>
  </class>
</cdb>
