<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/fdb.xsd">
  <frame id="fpa" label="Field Point Analog Input(FP-AI-100)" reference="http://ciax.sum.naoj.org/man/fieldpoint/fp-1000prm-321632c.pdf" ccmethod="sum" wait="0">
    <serial databit="8" flowctl="NONE" parity="NONE" speed="115200" stopbit="1" />
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <code label="Prefix">&gt;01</code>
      <select>
        <!--String Length must be <32-->
        <command id="getstat" label="Get status" response="getstat" type="stat">
          <code>!F00FF</code>
        </command>
        <command id="setr1" label="Set range +-1.2V" response="default" type="config">
          <code>!D00FF</code>
          <repeat from="1" to="8">
            <code>0000107</code>
          </repeat>
        </command>
        <command id="setr6" label="Set range +-6V" response="default" type="config">
          <code>!D00FF</code>
          <repeat from="1" to="8">
            <code>0000105</code>
          </repeat>
        </command>
        <command id="setr15" label="Set range +-15V" response="default" type="config">
          <code>!D00FF</code>
          <repeat from="1" to="8">
            <code>0000112</code>
          </repeat>
        </command>
      </select>
      <code>??</code>
      <code encode="chr">13</code>
      <code encode="chr">10</code>
    </cmdframe>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field label="Prefix" length="1">A</field>
      <ccrange>
        <select>
          <response id="getstat">
            <array assign="stat" decode="hexstr" label="Status" length="4">
              <index range="0:7" size="8" />
            </array>
          </response>
          <response id="default" />
        </select>
      </ccrange>
      <field assign="cc" decode="hexstr" label="BCC" length="2" />
    </rspframe>
  </frame>
</fdb>
