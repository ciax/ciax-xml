<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="psp" label="Power Supply Instek PSP-603">
    <spec type="Power Supply" mfr="Instek" model="PSP-603" reference="http://www.bellnw.com/images/man/0685.pdf"/>
    <stream wait="0.3" terminator="\r\n"/>
    <serial databit="8" flowctl="NONE" parity="NONE" speed="2400" stopbit="1"/>
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <body/>
      <char length="1">0xd</char>
    </cmdframe>
    <command xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <group id="gst" caption="Status">
        <item id="getstat" label="Get Status" response="getstat" type="stat">
          <string>L</string>
        </item>
      </group>
      <group id="gcfg" caption="Configuration">
        <item id="setvl" label="Set Voltage Limit [DD]" type="config">
          <par_num>0:20</par_num>
          <string>SU </string>
          <string format="%02d">$1</string>
        </item>
        <item id="setal" label="Set Current Limit [D.DD]" type="config">
          <par_num>0:&lt;10</par_num>
          <string>SI </string>
          <string format="%04.2f">$1</string>
        </item>
        <item id="setpl" label="Set Power Limit [DDD]" type="config">
          <par_num>0:&lt;200</par_num>
          <string>SP </string>
          <string format="%03d">$1</string>
        </item>
      </group>
      <group id="gact" caption="Action">
        <item id="setv" label="Set Voltage [DD.DD]" type="config">
          <par_num>0:&lt;100</par_num>
          <string>SV </string>
          <string format="%05.2f">$1</string>
        </item>
        <item id="pon" label="OUTPUT ON" type="action">
          <string>KOE</string>
        </item>
        <item id="poff" label="OUTPUT OFF" type="action">
          <string>KOD</string>
        </item>
      </group>
    </command>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <body/>
    </rspframe>
    <response xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <item id="getstat">
        <string>V</string>
        <string assign="v" label="Output(V)" length="5"/>
        <string>A</string>
        <string assign="a" label="Output(A)" length="5"/>
        <string>W</string>
        <string assign="w" label="Output(W)" length="5"/>
        <string>U</string>
        <string assign="u" label="Limit(V)" length="2"/>
        <string>I</string>
        <string assign="i" label="Limit(A)" length="4"/>
        <string>P</string>
        <string assign="p" label="Limit(W)" length="3"/>
        <string>F</string>
        <string assign="f1" label="Output(ON/OFF)" length="1"/>
        <string assign="f2" label="OverHeat" length="1"/>
        <string assign="f3" label="ADJ Fine" length="1"/>
        <string assign="f4" label="ADJ Unlock" length="1"/>
        <string assign="f5" label="Remote" length="1"/>
        <string assign="f6" label="Local Lock" length="1"/>
      </item>
    </response>
    <field xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <var id="v" label="Output(V)"/>
      <var id="a" label="Output(A)"/>
      <var id="w" label="Output(W)"/>
      <var id="u" label="Limit(V)"/>
      <var id="i" label="Limit(A)"/>
      <var id="p" label="Limit(W)"/>
      <var id="f1" label="Output(ON/OFF)"/>
      <var id="f2" label="OverHeat"/>
      <var id="f3" label="ADJ Fine"/>
      <var id="f4" label="ADJ Unlock"/>
      <var id="f5" label="Remote"/>
      <var id="f6" label="Local Lock"/>
    </field>
  </frame>
</fdb>
