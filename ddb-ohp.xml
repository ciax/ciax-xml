<?xml version="1.0" encoding="iso-8859-1"?>
<ddb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/ddb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/ddb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <device id="ohp" label="OMEGA Helium Pressure Sensor">
    <serial databit="7" flowctl="NONE" parity="ODD" speed="9600" stopbit="1" />
    <cmdselect>
      <command id="getstat" label="Get Status" type="stat">
        <data>*X01</data>
      </command>
      <command id="setd" label="Set default" type="config">
        <data>*W1F14</data>
      </command>
      <command id="init" label="Initialize" type="config">
        <data>*Z02</data>
      </command>
    </cmdselect>
    <cmdframe>
      <selected />
      <data encode="chr">13</data>
    </cmdframe>
    <rspselect>
      <response id="getstat">
        <field length="3" label="Prefix">
          <verify>X01</verify>
        </field>
        <field length="5" assign="pres"></field>
      </response>
      <response id="setd">
        <field length="3" label="Command">
          <verify>W1F</verify>
        </field>
      </response>
      <response id="init">
        <field length="3" label="Command">
          <verify>Z02</verify>
        </field>
      </response>
    </rspselect>
    <rspframe>
      <selected />
    </rspframe>
  </device>
</ddb>