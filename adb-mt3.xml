<?xml version="1.0" encoding="iso-8859-1"?>
<adb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/adb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/adb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object id="mt3" label="MOIRCS Pressure Pump">
    <comm>
      <class>tpg</class>
      <client>socat - tcp:celaeno:4011</client>
      <server>socat - udp-l:24111,reuseaddr,fork</server>
    </comm>
    <command>
      <session id="upd" label="Update Status"></session>
    </command>
    <status>
      <group>
        <title ref="st1" symbol="tpg:stat">Turbo Pump Stat</title>
        <title ref="ch1">Turbo Pump (Torr)</title>
      </group>
      <group>
        <title ref="st2" symbol="tpg:stat">Scroll Pump Stat</title>
        <title ref="ch2">Scroll Pump (Torr)</title>
      </group>
    </status>
  </object>
</adb>