<?xml version="1.0" encoding="utf-8"?>
<cdb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml/cdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <class frame="k3n" id="k3n" label="Helium Compressor Coolant Flow">
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbc">
      <session id="upd" label="Update Status">
        <statement command="getstat" />
      </session>
    </commands>
    <status xmlns="http://ciax.sum.naoj.org/ciax-xml/cdbs">
      <value id="err" label="STAT">
        <string ref="rcode" />
        <string ref="ecode" />
      </value>
      <value id="flow" label="Helium Compressor Coolant Flow(l/min)">
        <float decimal="2" format="%4.2f" ref="stat" />
      </value>
    </status>
    <symbol xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
      <case id="err">
        <regexp class="hide" msg="OK">000000</regexp>
        <regexp class="alarm" msg="Error">.</regexp>
      </case>
      <case id="flow" type="FLOAT">
        <range class="alarm" msg="HH">6:</range>
        <range class="warn" msg="H">5:&lt;6</range>
        <range class="normal" msg="M">4:&lt;5</range>
        <range class="warn" msg="L">3:&lt;4</range>
        <range class="alarm" msg="LL">:&lt;3</range>
      </case>
    </symbol>
  </class>
</cdb>