<?xml version="1.0" encoding="iso-8859-1"?>
<odb xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/odb.xsd" xmlns="http://ciax.sum.naoj.org/ciax-xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <object client="socat - tcp:ltt-i:4003" device="k3n" id="cf1" label="S1 Helium Compressor Coolant Flow" server="socat - udp-l:45601,reuseaddr,fork">
    <selection>
      <session id="upd" label="Update Status">
        <statement>
          <cmd>getstat</cmd>
        </statement>
      </session>
    </selection>
    <status>
      <var id="rcode" label="Response Code" symbol="rcode">
        <string field="rcode" />
      </var>
      <var id="ecode" label="Exit Code" symbol="ecode">
        <string field="ecode" />
      </var>
      <var id="flow" label="CS-2(S1) Coolant Flow(l/min)" symbol="flow">
        <float decimal="2" field="stat" format="%4.2f" />
      </var>
    </status>
    <symbols>
      <symbol id="rcode" label="Response Code" type="ENUM">
        <case hl="hide" msg="OK">0000</case>
        <case hl="alarm" msg="Cmd Length Over">1001</case>
        <case hl="alarm" msg="Cmd Length Short">1002</case>
        <case hl="alarm" msg="Parameter Error">1100</case>
        <case hl="alarm" msg="Area Error">1101</case>
        <case hl="alarm" msg="Start Address Error">1103</case>
        <case hl="alarm" msg="Action Error">2203</case>
        <case msg="ResponseCode Error" hl="alarm"></case>
      </symbol>
      <symbol id="ecode" label="ExitCode" type="ENUM">
        <case hl="hide" msg="OK">00</case>
        <case hl="alarm" msg="CMD Error">0F</case>
        <case hl="alarm" msg="Parity Error">10</case>
        <case hl="alarm" msg="Fleming Error">11</case>
        <case hl="alarm" msg="Over Run Error">12</case>
        <case hl="alarm" msg="BCC Error">13</case>
        <case hl="alarm" msg="Format Error">14</case>
        <case hl="alarm" msg="Sub Address Error">16</case>
        <case hl="alarm" msg="Frame Length Error">18</case>
        <case msg="Unknown" hl="alarm"></case>
      </symbol>
      <symbol id="flow" type="FLOAT">
        <range hl="alarm" msg="HH">6:</range>
        <range hl="warn" msg="H">5:&lt;6</range>
        <range hl="normal" msg="M">4:&lt;5</range>
        <range hl="warn" msg="L">3:&lt;4</range>
        <range hl="alarm" msg="LL">:&lt;3</range>
      </symbol>
    </symbols>
  </object>
</odb>