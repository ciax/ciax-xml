<?xml version="1.0" encoding="iso-8859-1"?>
<cdb xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ciax.sum.naoj.org/ciax-xml" xsi:schemaLocation="http://ciax.sum.naoj.org/ciax-xml/schema/cdb.xsd">
  <class id="hcc" device="bbe" label="Helium Compressor Control">
    <controls>
      <session id="upd" label="Update Status">
        <command>
          <text>getstat</text>
        </command>
      </session>
      <session id="on_0" label="[CH0] ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x01</text>
        </command>
      </session>
      <session id="off_0" label="[CH0] OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xfffe</text>
        </command>
      </session>
      <session id="on_1" label="CH1 ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x10</text>
        </command>
      </session>
      <session id="off_1" label="CH1 OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xffef</text>
        </command>
      </session>
      <session id="on_2" label="CH2 ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x100</text>
        </command>
      </session>
      <session id="off_2" label="CH2 OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xfeff</text>
        </command>
      </session>
      <session id="on_3" label="CH3 ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x1000</text>
        </command>
      </session>
      <session id="off_3" label="CH3 OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xefff</text>
        </command>
      </session>
      <session id="on_01" label="[CH0 and CH1] ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x0011</text>
        </command>
      </session>
      <session id="off_01" label="[CH0 and CH1] OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xffee</text>
        </command>
      </session>
      <session id="on_21" label="CH2 and CH1 ON">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="or">0x0110</text>
        </command>
      </session>
      <session id="off_23" label="CH2 and CH3 OFF">
        <command>
          <text>getstat</text>
        </command>
        <command>
          <text>output</text>
          <text ref="stat" operator="and">0xeeff</text>
        </command>
      </session>
    </controls>
    <status>
      <var id="0sw" label="[CH0] HC Switch" type="ENUM">
        <fields>
          <binary ref="stat" bit="0" />
        </fields>
        <symbol>
          <enum hl="warn" msg="OFF">0</enum>
          <enum msg="ON" hl="normal">1</enum>
        </symbol>
      </var>
      <var id="0dr" label="[CH0] HC Drive" type="ENUM">
        <fields>
          <binary ref="stat" bit="1" />
        </fields>
        <symbol>
          <enum hl="warn" msg="OFF">1</enum>
          <enum msg="ON" hl="normal">0</enum>
        </symbol>
      </var>
      <var id="0tm" label="[CH0] HC TMP Alarm" type="ENUM">
        <fields>
          <binary ref="stat" bit="2" />
        </fields>
        <symbol>
          <enum hl="hide" msg="OK">1</enum>
          <enum msg="ALARM" hl="alarm">0</enum>
        </symbol>
      </var>
      <var id="0pr" label="[CH0] HC PRS Alarm" type="ENUM">
