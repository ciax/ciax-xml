<?xml version="1.0" encoding="utf-8"?>
<sdb xmlns="http://ciax.sum.naoj.org/ciax-xml/sdb">
  <symbol id="share" label="Shared">
    <table id="iid_tsc" label="Instrument ID by TSC">
      <pattern class="warn" msg="NONE">^0$</pattern>
      <pattern class="normal" msg="IRCS">^1$</pattern>
      <pattern class="normal" msg="CIAO">^3$</pattern>
      <pattern class="normal" msg="FOCAS">5</pattern>
      <pattern class="normal" msg="COMICS">7</pattern>
      <pattern class="normal" msg="MC-BOX">10</pattern>
      <pattern class="normal" msg="SWIMS">11</pattern>
      <pattern class="normal" msg="MOIRCS">12</pattern>
      <pattern class="normal" msg="CIAX3">13</pattern>
      <default class="warn" msg="N/A"/>
    </table>
    <table id="iid_ac" label="Instrument ID by Auto Connector">
      <pattern class="warn" msg="NONE">0</pattern>
      <pattern class="normal" msg="CIAX3">1</pattern>
      <pattern class="normal" msg="COMICS">2</pattern>
      <pattern class="normal" msg="CIAO">3</pattern>
      <pattern class="normal" msg="FOCAS">4</pattern>
      <pattern class="normal" msg="IRCS">5</pattern>
      <pattern class="normal" msg="N/A">6</pattern>
      <pattern class="normal" msg="MOIRCS">7</pattern>
      <pattern class="normal" msg="MC-BOX">8</pattern>
      <pattern class="normal" msg="MP-CS2">9</pattern>
      <pattern class="normal" msg="SWIMS">10</pattern>
      <default class="warn" msg="OTHER"/>
    </table>
    <table id="normal">
      <pattern class="normal" msg="ON">1</pattern>
      <pattern class="normal" msg="OFF">0</pattern>
    </table>
    <table id="warn">
      <pattern class="warn" msg="ON">1</pattern>
      <pattern class="normal" msg="OFF">0</pattern>
    </table>
    <table id="off_warn">
      <pattern class="normal" msg="ON">1</pattern>
      <pattern class="warn" msg="OFF">0</pattern>
    </table>
    <table id="alarm">
      <pattern class="alarm" msg="ON">1</pattern>
      <pattern class="hide" msg="OFF">0</pattern>
    </table>
    <table id="off_alarm">
      <pattern class="hide" msg="ON">1</pattern>
      <pattern class="alarm" msg="OFF">0</pattern>
    </table>
    <table id="action">
      <pattern class="active" msg="ON">1</pattern>
      <pattern class="normal" msg="OFF">0</pattern>
    </table>
    <table id="hide">
      <pattern class="hide" msg="ON">1</pattern>
      <pattern class="hide" msg="OFF">0</pattern>
    </table>
    <table id="nonzero">
      <pattern class="normal" msg="OFF">0</pattern>
      <default class="warn" msg="ON"/>
    </table>
    <table id="control_code">
      <pattern class="normal" msg="ACK">6</pattern>
      <pattern class="alarm" msg="NAK">21</pattern>
    </table>
    <table id="ctl_mode">
      <pattern class="normal" msg="AUTO">2</pattern>
      <pattern class="alarm" msg="MANUAL">1</pattern>
      <pattern class="alarm" msg="N/A">0</pattern>
    </table>
