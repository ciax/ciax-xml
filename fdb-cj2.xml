<?xml version="1.0" encoding="utf-8"?>
<fdb xmlns="http://ciax.sum.naoj.org/ciax-xml/fdb">
  <frame id="cj2" label="OMRON CJ2M-CPU33" endian="big" version="1">
    <cmdframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <char length="1" label="FINS Header:ICF">0x80</char>
      <char length="1" label="FINS Header:RSV">0x00</char>
      <char length="1" label="FINS Header:GCT">0x02</char>
      <char length="1" label="FINS Header:DNA">0x00</char>
      <char length="1" label="FINS Header:DA1">${ipr}</char>
      <char length="1" label="FINS Header:DA2">0x00</char>
      <char length="1" label="FINS Header:SNA">0x00</char>
      <char length="1" label="FINS Header:SA1">${ipl}</char>
      <char length="1" label="FINS Header:SA2">0x00</char>
      <char length="1" label="FINS Header:SID">0xFF</char>
      <body />
    </cmdframe>
    <commands xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbc">
      <command id="getstat" label="Get all status" response="getstat" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x01</char>
        <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x0A</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x13</char>
      </command>
      <command id="getcmd" label="Get command bit" response="getcmd" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x01</char>
        <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x00</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x04</char>
      </command>
      <command id="geterr" label="Get Error Code" response="geterr" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x01</char>
        <char length="1" label="FINS Param:Memory type(D)">0x82</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x00</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x01</char>
      </command>
      <command id="getzlen" label="Get Z Sensor Length" response="getzlen" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x01</char>
        <char length="1" label="FINS Param:Memory type(D)">0x82</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x0A</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x0A</char>
      </command>
      <command id="setbit" label="Set Int/Cmd Bit" response="cmdrsp" type="stat">
        <par_num>0:3</par_num>
        <par_num>0:15</par_num>
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x02</char>
        <char length="1" label="FINS Param:Memory type(W)">0x31</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">$1</char>
        <char length="1" label="FINS Param:Bit Number">$2</char>
        <char length="1" label="FINS Param:Bit Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Bit Data Length (L)">0x01</char>
        <char length="1" label="FINS Param:Bit Data">0x01</char>
      </command>
      <command id="clrbit" label="Clear Int/Cmd Bit" response="cmdrsp" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x02</char>
        <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x00</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x04</char>
        <char length="2" label="FINS Param:Bit Data">0x00</char>
        <char length="2" label="FINS Param:Bit Data">0x00</char>
        <char length="2" label="FINS Param:Bit Data">0x00</char>
        <char length="2" label="FINS Param:Bit Data">0x00</char>
      </command>
      <command id="clrint" label="Clear Interrupt Bit" response="cmdrsp" type="stat">
        <char length="1" label="FINS Command:MRC">0x01</char>
        <char length="1" label="FINS Command:SRC">0x02</char>
        <char length="1" label="FINS Param:Memory type(W)">0xB1</char>
        <char length="1" label="FINS Param:Start Address(H)">0x00</char>
        <char length="1" label="FINS Param:Start Address(L)">0x00</char>
        <char length="1" label="FINS Param:Bit Number">0x00</char>
        <char length="1" label="FINS Param:Data Length (H)">0x00</char>
        <char length="1" label="FINS Param:Data Length (L)">0x01</char>
        <char length="2" label="FINS Param:Bit Data">0x00</char>
      </command>
    </commands>
    <rspframe xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <field decode="unsigned" label="FINS Header:ICF" length="1">0xc0</field>
      <field decode="unsigned" label="FINS Header:RSV" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:GCT" length="1">0x02</field>
      <field decode="unsigned" label="FINS Header:DNA" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:DA1" length="1" assign="ipl" />
      <field decode="unsigned" label="FINS Header:DA2" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SNA" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SA1" length="1" assign="ipr" />
      <field decode="unsigned" label="FINS Header:SA2" length="1">0x00</field>
      <field decode="unsigned" label="FINS Header:SID" length="1">0xFF</field>
      <body />
    </rspframe>
    <responses xmlns="http://ciax.sum.naoj.org/ciax-xml/fdbr">
      <response id="getstat">
        <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
        <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
        <array assign="w" decode="unsigned" label="Status" length="2">
          <index range="0:12" size="13" />
        </array>
      </response>
      <response id="getcmd">
        <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
        <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
        <array assign="c" decode="unsigned" label="Status" length="2">
          <index range="0:3" size="4" />
        </array>
      </response>
      <response id="geterr">
        <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
        <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
        <field assign="err" decode="unsigned" label="FINS Command:Error Code" length="2"></field>
      </response>
      <response id="getzlen">
        <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:SRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
        <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
        <array assign="d" decode="signed" label="Status" length="2">
          <index range="0:9" size="10" />
        </array>
      </response>
      <response id="cmdrsp" label="command resp">
        <field decode="unsigned" label="FINS Command:MRC" length="1">0x01</field>
        <field decode="unsigned" label="FINS Command:SRC" length="1">0x02</field>
        <field decode="unsigned" label="FINS Command:MRES" length="1">0x00</field>
        <field decode="unsigned" label="FINS Command:SRES" length="1">0x00</field>
      </response>
      <response id="dmy" label="for csv daemon">
        <field decode="unsigned" label="Local IP (CIAX)" length="1" assign="ipl">1</field>
        <field decode="unsigned" label="Remote IP (DSO)" length="1" assign="ipr">32</field>
        <field decode="unsigned" label="Exe Flag" length="1" assign="exe">0</field>
      </response>
    </responses>
  </frame>
</fdb>